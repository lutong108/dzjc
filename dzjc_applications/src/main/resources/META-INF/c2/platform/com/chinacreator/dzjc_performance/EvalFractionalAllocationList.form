  <form:scripts>buttonAdd_click:function(rowId,rowData,tableData){&#xD;
	Modal.open(&quot;f/dzjc_performance/createEvalFractionalAllocation&quot;,{},function(data){&#xD;
		$view.list.refresh();&#xD;
	});&#xD;
},&#xD;
buttonEdit_click:function(rowId,rowData,tableData){&#xD;
	if(rowId){&#xD;
		Modal.open(&quot;f/dzjc_performance/updateEvalFractionalAllocation&quot;,{id:rowId},function(data){&#xD;
			$view.list.refresh();&#xD;
		});&#xD;
	}else{&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}&#xD;
},&#xD;
buttonDelete_click:function(rowId,rowData,tableData){&#xD;
	var ids = $view.list.getSelectRowIds() ;&#xD;
	if(ids.length==0){&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}else{&#xD;
		Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},&#xD;
			function(data){&#xD;
				if(ids.length==1){&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_performance/v1/EvalFractionalAllocation/&quot;+ids[0]).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				}else{&#xD;
					 var queryStr=&quot;&quot;;&#xD;
					 for (var i =0; i&lt; ids.length;i++){&#xD;
						if(i==0){&#xD;
						   queryStr+=&quot;id=&quot;+ids[i] ;&#xD;
						}else{&#xD;
						   queryStr+=&quot;&amp;id=&quot;+ids[i];&#xD;
						}&#xD;
					  }&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_performance/v1/EvalFractionalAllocation?&quot;+queryStr).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				 }&#xD;
		});&#xD;
	}&#xD;
},

allocationType_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(cellvalue==1){&#xD;
		cellvalue='扣分';&#xD;
	}else if(cellvalue==2){&#xD;
		cellvalue='加分';&#xD;
	}else{&#xD;
		cellvalue='';&#xD;
	}&#xD;
	return cellvalue;
},

button_click:function($event){
	var queryParams = [];&#xD;
	debugger;&#xD;
	if($model.list.allocationType != '' &amp;&amp; $model.list.allocationType != null){&#xD;
		queryParams.push({field:'allocationType',op:'eq',data:$model.list.allocationType});&#xD;
	}&#xD;
	&#xD;
	if($model.list.tableName != '' &amp;&amp; $model.list.tableName != null){&#xD;
		queryParams.push({field:'tableName',op:'eq',data:$model.list.tableName});&#xD;
	}&#xD;
	if($model.list.allocationExamination != '' &amp;&amp; $model.list.allocationExamination != null){&#xD;
		queryParams.push({field:'allocationExamination',op:'eq',data:$model.list.allocationExamination});&#xD;
	}&#xD;
	if(queryParams.length != 0){&#xD;
		var filters = {groupOp:'AND',rules:queryParams};&#xD;
		$view.list.refresh({filters:filters});&#xD;
	}else{&#xD;
		$view.list.refresh({filters:filters});&#xD;
	}
},
itemTree1_nodeClick:function(event,treeId,treeNode,clickFlag){
	$model.list.allocationExamination=treeNode.name;
},
button1_click:function($event){&#xD;
	$view.mainForm.clean();
	$view.list.refresh({null:null});
},
selectSimple4_nodeClick:function(event,treeId,treeNode,clickFlag){&#xD;
			debugger;&#xD;
			$view.selectTree3.clean();&#xD;
			$model.list.tableName=treeNode.name;&#xD;
			$view.selectTree3.getTree().refresh2({tableName:treeNode.id});
			
},

selectTree3_nodeClick:function(event,treeId,treeNode,clickFlag){
	$model.list.allocationExamination=treeNode.name;
}</form:scripts>
