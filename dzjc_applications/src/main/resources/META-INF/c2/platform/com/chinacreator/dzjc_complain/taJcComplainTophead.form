  <form:scripts>//查询
query_button_click:function($event){

	var queryParams = new Array();

	if($model.list.topTitle!= '' &amp;&amp; $model.list.topTitle!= null){
		queryParams.push({field:&quot;topTitle&quot;,op:&quot;cn&quot;,data:$model.list.topTitle}) ;
	}
	
	if(beginDate.value!= '' &amp;&amp; beginDate.value!= null){
		queryParams.push({field:&quot;createTime&quot;,op:&quot;ge&quot;,data:beginDate.value}) ;
	}
	
	if(endDate.value!= '' &amp;&amp; endDate.value!= null){
		if(beginDate.value > endDate.value){
			Messenger.error(&quot;开始时间不能大于结束时间！&quot;);
			return;
		}
		queryParams.push({field:&quot;createTime&quot;,op:&quot;le&quot;,data:endDate.value}) ;
	}
	
	if($model.list.isPublic == 'Y' || $model.list.isPublic == 'N'){
		queryParams.push({field:&quot;isPublic&quot;,op:&quot;eq&quot;,data:$model.list.isPublic});
	}
	
	if(queryParams.length != 0 ){
		var filters={groupOp:&quot;AND&quot;,rules:queryParams}
		$view.list.refresh({filters:filters});
	}else{
		$view.list.refresh({filters:filters});
	}
},

//重置
clean_button_click:function($event){
	$view.mainForm.clean();
	$view.list.refresh({});
},

//新增
buttonAdd_click:function(rowId,rowData,tableData){
	Modal.open(&quot;f/dzjc_complain/taJcComplainTopheadAdd&quot;,{},function(data){
		$view.list.refresh({});
	});
},

//删除
buttonDelete_click:function(rowId,rowData,tableData){
	var ids = $view.list.getSelectRowIds() ;
	if(!ids.length){
		Messenger.error(&quot;请选中表格项!&quot;);
	}else{
		Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},
				function(data){
			if(ids.length==1){
				$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_complain/v1/taJcComplainTopheadlines/&quot;+ids[0]).success(function(){
					$view.list.refresh();
				});
			}else{
				var queryStr=&quot;&quot;;
				for (var i =0; i&lt; ids.length;i++){
					if(i==0){
						queryStr+=&quot;id=&quot;+ids[i] ;
					}else{
						queryStr+=&quot;&amp;id=&quot;+ids[i];
					}
				}
				$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_complain/v1/taJcComplainTopheadlines?&quot;+queryStr).success(function(){
					$view.list.refresh();
				});
			}
		});
	}
},

//修改
list_ondblClickRow:function(rowid,iRow,iCol,e){
	Modal.open(&quot;f/dzjc_complain/taJcComplainTopheadUpdate&quot;,{topId:rowid},function(data){
		$view.list.refresh();
	});
},

isPublic_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(cellvalue=='Y'){  
		return &quot;是&quot;
	}else if(cellvalue=='N'){
		return &quot;否&quot;
	}else{
		return &quot;&quot;;
	}
}</form:scripts>
