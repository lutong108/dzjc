  <form:scripts>buttonAdd_click:function(rowId,rowData,tableData){&#xD;
	Modal.open(&quot;f/dzjc_ruleEngine/createMessageOrgRef&quot;,{},function(data){&#xD;
		$view.list.refresh();&#xD;
	});&#xD;
},&#xD;
buttonEdit_click:function(rowId,rowData,tableData){&#xD;
	if(rowId){&#xD;
		Modal.open(&quot;f/dzjc_ruleEngine/updateMessageOrgRef&quot;,{id:rowId},function(data){&#xD;
			$view.list.refresh();&#xD;
		});&#xD;
	}else{&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}&#xD;
},&#xD;
buttonDelete_click:function(rowId,rowData,tableData){&#xD;
	var ids = $view.list.getSelectRowIds() ;&#xD;
	if(ids.length==0){&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}else{&#xD;
		Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},&#xD;
			function(data){&#xD;
				if(ids.length==1){&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;message/v1/orgref/&quot;+ids[0]).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				}else{&#xD;
					 var queryStr=&quot;&quot;;&#xD;
					 for (var i =0; i&lt; ids.length;i++){&#xD;
						if(i==0){&#xD;
						   queryStr+=&quot;id=&quot;+ids[i] ;&#xD;
						}else{&#xD;
						   queryStr+=&quot;&amp;id=&quot;+ids[i];&#xD;
						}&#xD;
					  }&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;message/v1/orgref?&quot;+queryStr).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				 }&#xD;
		});&#xD;
	}&#xD;
},

button_click:function($event){
	$model.entity.$save(function(){&#xD;
		Messenger.post({type:'success',message:'新增成功'});&#xD;
		$view.list.refresh();&#xD;
	});
},

button1_click:function($event){
	var queryParams = new Array();&#xD;
	if(IsNotSpace($model.streetName)){&#xD;
		queryParams.push({field:&quot;streetOrgName&quot;,op:&quot;cn&quot;,data:$model.streetName}) ;&#xD;
	}&#xD;
	if(queryParams.length != 0 ){&#xD;
		var filters={groupOp:&quot;AND&quot;,rules:queryParams}&#xD;
		$view.list.refresh({filters:filters});&#xD;
	}else{&#xD;
		$view.list.refresh({filters:null});&#xD;
	}
},


selectTree_nodeClick:function(event,treeId,treeNode,clickFlag){
	$model.entity.acceptOrgName=treeNode.name;
},

selectTree1_nodeClick:function(event,treeId,treeNode,clickFlag){
	$model.entity.streetOrgName=treeNode.name;
}</form:scripts>
