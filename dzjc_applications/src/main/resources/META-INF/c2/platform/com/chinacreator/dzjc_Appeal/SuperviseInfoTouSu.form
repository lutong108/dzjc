  <form:scripts>//材料名称样式
attachName_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	return cellvalue.substring(cellvalue.lastIndexOf(&quot;$$&quot;)+2);
},

//数据源加载后
page_afterDataSourceLoad:function(){

	if($model.entity.expire_date !=null &amp;&amp; $model.entity.expire_date !=&quot;&quot;){
		var date = new Date($model.entity.expire_date);
		var commonTime = date.toLocaleString('chinese',{hour12:false});
		$model.entity.expire_date = commonTime;
	}
	
	console.log(&quot;$params.id--->&quot;,$params.id);
	console.log(&quot;$params.superviseInfoId--->&quot;,$params.superviseInfoId);

	//发牌结果
	var state = $model.entity.supervise_result_name;
	
	if(state == &quot;黄牌&quot; || state == &quot;红牌&quot; ){
		$(&quot;#jcss&quot;).show();
		$(&quot;#close&quot;).hide();
	
	}else{

		$(&quot;#jcss&quot;).hide();
		$(&quot;#close&quot;).show();
	}
	
	if($params.suState == &quot;已申诉&quot;){
	
		$(&quot;#jcss&quot;).hide();
		$(&quot;#close&quot;).show();
	}
	
	$(&quot;#widgetbox&quot;).hide();
	$(&quot;#commit&quot;).hide();
	$(&quot;#cancel&quot;).hide();
},

//关闭
button6_click:function($event){
	Modal.instance.close();
},


//点击监管申诉
jcss_click:function($event){
	$(&quot;#widgetbox&quot;).show();
	$(&quot;#commit&quot;).show();
	$(&quot;#cancel&quot;).show();
	
	$(&quot;#jcss&quot;).hide();
},

//点击取消
cancel_click:function($event){
	
	$(&quot;#widgetbox&quot;).hide();
	$(&quot;#commit&quot;).hide();
	$(&quot;#cancel&quot;).hide();
	
	$(&quot;#jcss&quot;).show();
},


//处理结果
caz_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	var html = &quot;&lt;a style='cursor: pointer;' ng-click='functions.appealInfo(rowObject);'>查看详情&lt;/a> &quot;;
	return html;
},

//申诉详情
appealInfo:function(rowObject){

	console.log(&quot;rowObject--->&quot;,rowObject);

	Modal.open(&quot;f/dzjc/TaJcWarningAppeal&quot;,{id:rowObject.id},function(data){
		$view.appeal.refresh({&quot;superviseInfoId&quot;:$params.id});
	});

},


//提交申诉
commit_click:function($event){
	
	if(!$model.appeal.appeal_content){
		Messenger.post({type:'error',message:'申诉内容不能为空'});
		$(&quot;#appeal_content&quot;).focus();
		return;
	}
	
	if($model.appeal.appeal_content.length > 250){
		Messenger.post({type:'error',message:'申诉内容不能超过250字符'});
		$(&quot;#appeal_content&quot;).focus();
		return;
	}
	
	
	var map ={
			&quot;superviseInfoId&quot;:$params.id,
			&quot;appeal_content&quot;:$model.appeal.appeal_content,
			&quot;id&quot;:$params.uuid
	};
	
	
	//提交
	$http.post(c2.base(&quot;service&quot;)+&quot;dzjc/v1/taJcWarningAppeal/zdy&quot;,map).success(function(data){
		$view.appeal.refresh({&quot;superviseInfoId&quot;:$params.superviseInfoId});
		$model.appeal.appeal_content=&quot;&quot;;
		$(&quot;#widgetbox&quot;).hide();
		$(&quot;#commit&quot;).hide();
		$(&quot;#cancel&quot;).hide();
		$(&quot;#jcss&quot;).hide();
		$(&quot;#close&quot;).show();
		Messenger.post({type:'success',message:'提交成功'});
	});
},

page_beforeDataSourceLoad:function($params){
	$params.uuid=uuid();
},

attachinfo_button_click:function($event){
	if(!IsSpace($model.entity.attachname)){
		$view.attach.submit({complainId:$params.uuid,businessType:&quot;3&quot;,attachname:$model.entity.attachname});
	}else{
		Messenger.post({type:'error',message:'请输入材料名称'});
	}
},
attachName_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	return cellvalue.substr(cellvalue.indexOf(&quot;$$&quot;)+2);
},
multipleFileInputIframe_onSuccess:function(data){
	$view.attachList.refresh({&quot;businessType&quot;:&quot;3&quot;,&quot;complainId&quot;:$params.uuid});
	$view.attachname.clean();
	$view.attach.reset();
},
multipleFileInputIframe_onError:function(data){
	Messenger.post({type:'error',message:'上传失败'});
},

createTime_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	return moment(cellvalue).format(&quot;YYYY-MM-DD&quot;);
},

colJqgrid_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	var htmls = 
				//&quot;&lt;a style='cursor: pointer;' ng-click='functions.attach_download(rowObject);'>下载&lt;/a> &amp;nbsp;&amp;nbsp;&quot;
			  &quot;&lt;a style='cursor: pointer;' ng-click='functions.attach_delete(rowObject);'>删除&lt;/a>&quot;;
	return htmls;
},
attach_download:function(rowObject){
	var url =&quot;/dzjc/ComplainAttachinfoServlet?attachId=&quot;+rowObject.attachId+&quot;&amp;fileName=&quot;+encodeURIComponent(encodeURIComponent(rowObject.attachName));
	window.location.href =url;
},
attach_delete:function(rowObject){
	Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},function(data){
		$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_complain/v1/taJcComplainAttachinfo/&quot;+rowObject.attachId).success(function(){
			$view.attachList.refresh({&quot;businessType&quot;:&quot;3&quot;,&quot;complainId&quot;:$params.uuid});
		});
	});
}</form:scripts>
</form:Form>
