  <form:scripts>

buttonAdd_click:function(rowId,rowData,tableData){
	Modal.open(&quot;f/dzjc_quartz/createQuartzInfo&quot;,{},function(data){&#xD;
		$view.list.refresh();&#xD;
	});
},

buttonEdit_click:function(rowId,rowData,tableData){
	var jobName=rowData.jobName;
	var jobGroup=rowData.jobGroup;
	var schedName=rowData.schedName;
	if(rowId){&#xD;
		Modal.open(&quot;f/dzjc_quartz/updateQuartzInfo&quot;,{jobName:jobName,jobGroup:jobGroup,schedName:schedName},function(data){&#xD;
			$view.list.refresh();&#xD;
		});&#xD;
	}else{&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}
},

buttonDelete_click:function(rowId,rowData,tableData){
	var jobName=rowData.jobName;
	var jobGroup=rowData.jobGroup;
	var triggerName=rowData.triggerName;
	var ids = $view.list.getSelectRowIds() ;
	&#xD;
	if(ids.length==0){&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}else if(ids.length>1){
		Messenger.error(&quot;只能选中一条表格项!&quot;);
	}else{&#xD;
		Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},&#xD;
			function(data){&#xD;
				$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_quartz/v1/quartzInfo/deleteQuartz&quot;,{jobName:jobName,jobGroup:jobGroup,triggerName:triggerName}).success(function(data){ 
			       if(data=='0'){
			    	  Messenger.post({type:'success',message:'job删除成功'});
			    	  $view.list.refresh();
			      }else if(data=='2'){
			    	  Messenger.post({type:'error',message:'job删除失败'});
			      }
			    });&#xD;
				&#xD;
		});&#xD;
	}
}</form:scripts>
