  <form:scripts>buttonAdd_click:function(rowId,rowData,tableData){&#xD;
	Modal.open(&quot;f/dzjc_log/createLogAudit&quot;,{},function(data){&#xD;
		$view.list.refresh();&#xD;
	});&#xD;
},&#xD;
buttonEdit_click:function(rowId,rowData,tableData){&#xD;
	if(rowId){&#xD;
		Modal.open(&quot;f/dzjc_log/updateLogAudit&quot;,{id:rowId},function(data){&#xD;
			$view.list.refresh();&#xD;
		});&#xD;
	}else{&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}&#xD;
},&#xD;
buttonDelete_click:function(rowId,rowData,tableData){&#xD;
	var ids = $view.list.getSelectRowIds() ;&#xD;
	if(ids.length==0){&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}else{&#xD;
		Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},&#xD;
			function(data){&#xD;
				if(ids.length==1){&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_log/LogAudit/&quot;+ids[0]).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				}else{&#xD;
					 var queryStr=&quot;&quot;;&#xD;
					 for (var i =0; i&lt; ids.length;i++){&#xD;
						if(i==0){&#xD;
						   queryStr+=&quot;id=&quot;+ids[i] ;&#xD;
						}else{&#xD;
						   queryStr+=&quot;&amp;id=&quot;+ids[i];&#xD;
						}&#xD;
					  }&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_log/LogAudit?&quot;+queryStr).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				 }&#xD;
		});&#xD;
	}&#xD;
},
&#xD;
//操作类型
opType_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){&#xD;
	var returnValue = &quot;&quot;;&#xD;
	if(cellvalue == '1'){&#xD;
		returnValue = &quot;新增&quot;;&#xD;
	}else if(cellvalue == '2'){&#xD;
		returnValue = &quot;编辑&quot;;&#xD;
	}else if(cellvalue == '3'){&#xD;
		returnValue = &quot;删除&quot;;&#xD;
	}else if(cellvalue == '4'){&#xD;
		returnValue = &quot;其他&quot;;&#xD;
	}else{&#xD;
		returnValue = &quot;&quot;;&#xD;
	}&#xD;
	return returnValue;
},
//查询
button_click:function($event){&#xD;
	var userName = $model.list.userName;&#xD;
	var opType = $model.list.opType;&#xD;
    var beginDate = $view.dateTimeRangePicker.getStartDatetimeObj().getValue();&#xD;
	var endDate = $view.dateTimeRangePicker.getEndDatetimeObj().getValue();&#xD;
	&#xD;
	$view.list.refresh({userName:userName,opType:opType,beginDate:beginDate,endDate:endDate});
},
//重置
button1_click:function($event){
	$model.list.userName = &quot;&quot;;&#xD;
    $model.list.opType = &quot;&quot;;&#xD;
    $params.beginDate = &quot;&quot;;&#xD;
 	$params.endDate = &quot;&quot;;&#xD;
	functions.initDateTimeRangePicker();&#xD;
	setTimeout(function(){&#xD;
		functions.button_click();&#xD;
	},500);
},&#xD;
//初始化控件&#xD;
initDateTimeRangePicker:function(){&#xD;
 	var date_ = new Date();  &#xD;
    date_.setDate(1); &#xD;
	$params.beginDate=date_;&#xD;
    var currentMonth=date_.getMonth();&#xD;
    var nextMonth=++currentMonth;&#xD;
 	var nextMonthFirstDay=new Date(date_.getFullYear(),nextMonth,1);&#xD;
 	var t=new Date(); &#xD;
 	t.setTime(nextMonthFirstDay-1000*60*60*24); &#xD;
 	$params.endDate=t;&#xD;
 	&#xD;
},
//数据源加载前
page_beforeDataSourceLoad:function($params){
	functions.initDateTimeRangePicker();
}</form:scripts>
</form:Form>
