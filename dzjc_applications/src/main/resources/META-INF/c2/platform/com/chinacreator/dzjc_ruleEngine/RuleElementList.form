  <form:scripts>buttonAdd_click:function(rowId,rowData,tableData){&#xD;
	Modal.open(&quot;f/dzjc_ruleEngine/createRuleElement&quot;,{},function(data){&#xD;
		$view.list.refresh();&#xD;
	});&#xD;
},&#xD;
buttonEdit_click:function(rowId,rowData,tableData){&#xD;
	var ids = $view.list.getSelectRowIds() ;&#xD;
	if(ids.length==0){&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}else if(ids.length==1){&#xD;
	&#xD;
		Modal.open(&quot;f/dzjc_ruleEngine/updateRuleElement&quot;,{'elemId':ids[0]},function(data){&#xD;
			$view.list.refresh();&#xD;
		});&#xD;
	}else{&#xD;
		Messenger.error(&quot;修改每次只能修改一项，请单选一项修改!&quot;);&#xD;
	}	&#xD;
},&#xD;
buttonDelete_click:function(rowId,rowData,tableData){&#xD;
	var ids = $view.list.getSelectRowIds() ;&#xD;
	if(ids.length==0){&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}else{&#xD;
		Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},&#xD;
			function(data){&#xD;
				if(ids.length==1){&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/ruleelement/&quot;+ids[0]).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				}else{&#xD;
					 var queryStr=&quot;&quot;;&#xD;
					 for (var i =0; i&lt; ids.length;i++){&#xD;
						if(i==0){&#xD;
						   queryStr+=&quot;id=&quot;+ids[i] ;&#xD;
						}else{&#xD;
						   queryStr+=&quot;&amp;id=&quot;+ids[i];&#xD;
						}&#xD;
					  }&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/ruleelement?&quot;+queryStr).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				 }&#xD;
		});&#xD;
	}&#xD;
},

query_button_click:function($event){
	var queryParams = new Array();&#xD;
&#xD;
	if($model.list.elemName!= '' &amp;&amp; $model.list.elemName!= null){&#xD;
		queryParams.push({field:&quot;elemName&quot;,op:&quot;cn&quot;,data:$model.list.elemName}) ;&#xD;
	}&#xD;
	if($model.list.businessTypeCode!= '' &amp;&amp; $model.list.businessTypeCode!= null){&#xD;
		queryParams.push({field:&quot;businessTypeCode&quot;,op:&quot;cn&quot;,data:$model.list.businessTypeCode}) ;&#xD;
	}&#xD;
	if($model.list.isValid!= '' &amp;&amp; $model.list.isValid!= null){&#xD;
		queryParams.push({field:&quot;isValid&quot;,op:&quot;eq&quot;,data:$model.list.isValid}) ;&#xD;
	}&#xD;
	if($model.list.isPublic!= '' &amp;&amp; $model.list.isPublic!= null){&#xD;
		queryParams.push({field:&quot;isPublic&quot;,op:&quot;eq&quot;,data:$model.list.isPublic}) ;&#xD;
	}&#xD;
	if(queryParams.length != 0 ){&#xD;
		var filters={groupOp:&quot;AND&quot;,rules:queryParams}&#xD;
		$view.list.refresh({filters:filters});&#xD;
	}else{&#xD;
		$view.list.refresh({});&#xD;
	}
},

reset_button_click:function($event){
	$view.mainForm.clean();&#xD;
	$view.list.refresh({});
},

list_ondblClickRow:function(rowid,iRow,iCol,e){
	Modal.open(&quot;f/dzjc_ruleEngine/updateRuleElement&quot;,{'elemId':rowid},function(data){&#xD;
		$view.list.refresh();&#xD;
	});
}</form:scripts>
