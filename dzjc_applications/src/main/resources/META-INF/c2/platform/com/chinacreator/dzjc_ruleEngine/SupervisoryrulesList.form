	}&#xD;
},&#xD;
buttonDelete_click:function(rowId,rowData,tableData){&#xD;
	var ids = $view.list.getSelectRowIds() ;&#xD;
	if(ids.length==0){&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}else{&#xD;
		Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},&#xD;
			function(data){&#xD;
				if(ids.length==1){&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/Supervisoryrules/&quot;+ids[0]).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				}else{&#xD;
					 var queryStr=&quot;&quot;;&#xD;
					 for (var i =0; i&lt; ids.length;i++){&#xD;
						if(i==0){&#xD;
						   queryStr+=&quot;id=&quot;+ids[i] ;&#xD;
						}else{&#xD;
						   queryStr+=&quot;&amp;id=&quot;+ids[i];&#xD;
						}&#xD;
					  }&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/Supervisoryrules?&quot;+queryStr).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				 }&#xD;
		});&#xD;
	}&#xD;
},

button_click:function($event){&#xD;
	var queryParams = new Array();&#xD;
&#xD;
	if($model.list.supervisoryRulesName!= '' &amp;&amp; $model.list.supervisoryRulesName!= null){&#xD;
		queryParams.push({field:&quot;supervisoryRulesName&quot;,op:&quot;cn&quot;,data:$model.list.supervisoryRulesName}) ;&#xD;
	}&#xD;
	if(queryParams.length != 0 ){&#xD;
		var filters={groupOp:&quot;AND&quot;,rules:queryParams}&#xD;
		&#xD;
		$params.cond = {&#xD;
			filters:filters&#xD;
		}&#xD;
		&#xD;
		$view.list.refresh({filters:filters});&#xD;
		}&#xD;
		$view.list.refresh({filters:filters});
},

