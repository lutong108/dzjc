  <form:scripts>

projectState_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
     if(cellvalue!=undefined &amp;&amp; cellvalue!=&quot;&quot;){
       var result = '&lt;a class=&quot;cell-btn&quot; style=&quot;cursor:pointer&quot; id=&quot;ymtb_'+options.rowId+'&quot; ng-mouseover=&quot;functions.showByslMsg(rowObject,'+options.rowId+')&quot; ng-click=&quot;functions.banJian_click(rowObject);&quot;>'+cellvalue+'&lt;/a>';
    	return result;
     }
     else 
    	 return &quot;&quot;;
},
showByslMsg:function(rowObject,rowId){
	var projectState=rowObject.projectState;
	var showId=&quot;#ymtb_&quot;+rowId;
	var msg=rowObject.notacceptReason;
	if(projectState==&quot;不予受理&quot;){
		var index=layer.tips(msg, showId, {
		  tips: [1, 'rgb(0,0,0)'],
		  time:1000000
		});
		$(showId).mouseout(function(){
			layer.close(index);
		});
	}
},
banJian_click:function(rowObject){
	 Modal.open(&quot;f/dzjc_union/InstanceProcList&quot;,{id:rowObject.instanceId},function(data){
			$view.list.refresh();
	 });
	
},

superviseResultNo_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(rowObject.superviseResultNo==&quot;1&quot;){
	return &quot;&lt;img style='margin-left: 7px; width:15px;'src='dzjc/images/table_icon02.gif' title='预警' alt='预警'  /> &quot;;
	}else if(rowObject.superviseResultNo==&quot;2&quot;){
	return &quot;&lt;img style='margin-left: 3px; width:15px;' src='dzjc/images/table_icon03.gif' title='黄牌' alt='黄牌'/>&quot;;
	}else if (rowObject.superviseResultNo==&quot;3&quot;){
	return &quot;&lt;img style='margin-left: 3px; width:15px;' src='dzjc/images/table_icon04.gif' title='红牌' alt='红牌'/>&quot;;
	} else {
	return &quot;&lt;img style='margin-left: 3px; width:15px;' src='dzjc/images/table_icon01.gif' title='正常' alt='正常'/>&quot;;
	} 
},

page_onload:function(){
	 
},

acceptSupResult_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(rowObject.acceptSupResult==&quot;2&quot;){
		return &quot;&lt;img style='margin-left: 3px; width:15px;' src='dzjc/images/table_icon03.gif' title='黄牌' alt='黄牌'/>&quot;;
	} else {
		return &quot;&lt;img style='margin-left: 3px; width:15px;' src='dzjc/images/table_icon01.gif' title='正常' alt='正常'/>&quot;;
	} 
},

button_click:function($event){
	Modal.instance.close();
}</form:scripts>
