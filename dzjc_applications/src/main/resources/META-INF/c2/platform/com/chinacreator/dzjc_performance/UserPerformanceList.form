  <form:scripts>buttonAdd_click:function(rowId,rowData,tableData){&#xD;
	Modal.open(&quot;f/dzjc_performance/createUserPerformance&quot;,{},function(data){&#xD;
		$view.list.refresh();&#xD;
	});&#xD;
},&#xD;
buttonEdit_click:function(rowId,rowData,tableData){&#xD;
	if(rowId){&#xD;
		Modal.open(&quot;f/dzjc_performance/updateUserPerformance&quot;,{id:rowId},function(data){&#xD;
			$view.list.refresh();&#xD;
		});&#xD;
	}else{&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}&#xD;
},&#xD;
buttonDelete_click:function(rowId,rowData,tableData){&#xD;
&#xD;
&#xD;
	var ids = $view.list.getSelectRowIds();&#xD;
	if(ids.length==0){&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
		return;&#xD;
	}&#xD;
	$params.user_id=ids;&#xD;
		$.ajax({ &#xD;
		url : c2.base(&quot;service&quot;)+&quot;dzjc_performance/v1/UserPerformance/getCheckList_isdelete&quot;, &#xD;
		data : {'user_id': $params.user_id},&#xD;
		dataType : &quot;json&quot;,&#xD;
		type : &quot;GET&quot;,&#xD;
		traditional: true,&#xD;
		async: false,&#xD;
		success : function(result) {&#xD;
				if(result[1]==1){&#xD;
					Messenger.error(&quot;选中的数据中包含已经考核的用户，不能删除&quot;);&#xD;
					return;&#xD;
				}else{&#xD;
					if(ids.length==0){&#xD;
					Messenger.error(&quot;请选中表格项!&quot;);&#xD;
				}else{&#xD;
						Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},&#xD;
							function(data){&#xD;
							if(ids.length==1){&#xD;
								$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_performance/v1/UserPerformance/&quot;+ids[0]).success(function(){&#xD;
									$view.list.refresh();&#xD;
								});&#xD;
							}else{&#xD;
								 var queryStr=&quot;&quot;;&#xD;
								 for (var i =0; i&lt; ids.length;i++){&#xD;
									if(i==0){&#xD;
									   queryStr+=&quot;id=&quot;+ids[i] ;&#xD;
									}else{&#xD;
									   queryStr+=&quot;&amp;id=&quot;+ids[i];&#xD;
									}&#xD;
								  }&#xD;
								$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_performance/v1/UserPerformance?&quot;+queryStr).success(function(){&#xD;
									$view.list.refresh();&#xD;
								});&#xD;
							 }&#xD;
								});&#xD;
							}&#xD;
								&#xD;
						}&#xD;
					&#xD;
				}&#xD;
		&#xD;
	});&#xD;
&#xD;
&#xD;
	&#xD;
},

sex_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(cellvalue==1){&#xD;
	cellvalue='男';&#xD;
	}else{&#xD;
	cellvalue='女'&#xD;
	}&#xD;
	return cellvalue;
},

page_beforeDataSourceLoad:function($params){
	$params.orgId = c2.subject.orgIds[0];
},

button_click:function($event){
	var queryParams = new Array();&#xD;
	&#xD;
	if($model.list.name!= '' &amp;&amp; $model.list.name!= null){&#xD;
		queryParams.push({field:&quot;name&quot;,op:&quot;cn&quot;,data:$model.list.name}) ;&#xD;
	}&#xD;
	if($model.list.orgName!= '' &amp;&amp; $model.list.orgName!= null){&#xD;
		queryParams.push({field:&quot;orgName&quot;,op:&quot;cn&quot;,data:$model.list.orgName}) ;&#xD;
	}&#xD;
	queryParams.push({field:&quot;orgId&quot;,op:&quot;eq&quot;,data:$params.orgId}) ;&#xD;
	&#xD;
	&#xD;
	if(queryParams.length != 0 ){&#xD;
		var filters={groupOp:&quot;AND&quot;,rules:queryParams}&#xD;
		&#xD;
		$params.cond = {&#xD;
			filters:filters&#xD;
		}&#xD;
		&#xD;
		$view.list.refresh({filters:filters});&#xD;
	}else{&#xD;
	&#xD;
		$params.cond = {&#xD;
			filters:filters&#xD;
		}&#xD;
	&#xD;
		$view.list.refresh({});&#xD;
	}
},

button1_click:function($event){
	$view.mainForm.clean();&#xD;
	var queryParams = new Array();&#xD;
	queryParams.push({field:&quot;orgId&quot;,op:&quot;eq&quot;,data:$params.orgId}) ;&#xD;
		&#xD;
	if(queryParams.length != 0 ){&#xD;
		var filters={groupOp:&quot;AND&quot;,rules:queryParams}&#xD;
		&#xD;
		$params.cond = {&#xD;
			filters:filters&#xD;
		}&#xD;
		&#xD;
		$view.list.refresh({filters:filters});&#xD;
	}else{&#xD;
	&#xD;
		$params.cond = {&#xD;
			filters:filters&#xD;
		}&#xD;
	&#xD;
		$view.list.refresh({});&#xD;
	}&#xD;
}</form:scripts>
