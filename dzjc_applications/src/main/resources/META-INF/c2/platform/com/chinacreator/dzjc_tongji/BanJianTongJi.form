  <form:scripts>
//初始化表格控件之后
list_afterInit:function(events){
	$view.list.setFrozenColumns('grid_areaOrgName');
},
//双击行
list_ondblClickRow:function(rowid,iRow,iCol,e){
	//1,非长沙市用户登录，双击&quot;多规合一&quot;数据，不处理
	if($params.isChangShaShiData &amp;&amp; rowid != null &amp;&amp; rowid.substring(0,6) == &quot;DuoGui&quot;){
		return;
	}
	
	//2,双击事件的执行代码
	var recoed = $view.list.getSingleSelectData();
	var orgLevel = $view.list.getSingleSelectData().orgLevelInt;
	if(orgLevel==2||orgLevel==3){
		$params.orgId = $view.list.getSingleSelectData().orgId;
		functions.loadData();
	}else{
		//Messenger.error(&quot;该数据无法操作&quot;);
	}
},
//重置
button1_click:function($event){
    $view.list.setGridParam({sortname:&quot;&quot;}, false);
    
	functions.initDateTimeRangePicker();
	setTimeout(function(){
		functions.initDateTimeRangePicker();functions.loadData();
	},500);
},
//初始化控件
initDateTimeRangePicker:function(){
	$params.orgId='';
	var date_ = new Date();  
    date_.setDate(1); 
	$params.beginDate=date_;
    var currentMonth=date_.getMonth();
    var nextMonth=++currentMonth;
 	var nextMonthFirstDay=new Date(date_.getFullYear(),nextMonth,1);
 	var t=new Date(); 
 	t.setTime(nextMonthFirstDay-1000*60*60*24); 
 	$params.endDate=t; 
},
//加载数据
loadData:function(){
	var beginDate = $view.dateTimeRangePicker.getStartDatetimeObj().getValue();
	var endDate = $view.dateTimeRangePicker.getEndDatetimeObj().getValue();
	$view.list.refresh({orgId:$params.orgId,beginDate:beginDate,endDate:endDate});
},
//数据源加载前
page_beforeDataSourceLoad:function($params){
	functions.initDateTimeRangePicker();
	
	//&quot;多规合一&quot;参数
	$params.duoGuiOrgMap;
	$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_tongji/v1/getSystemAreaCode&quot;).success(function(result){
		if(result.success){	
			var SYSTEM_AREACODE_VALUE = result.SYSTEM_AREACODE_VALUE;
			var SYSTEM_DUOGUI_AREACODE_VALUE = result.SYSTEM_DUOGUI_AREACODE_VALUE;
			//只有SYSTEM_AREACODE_VALUE为长沙市时展示相关操作
			if(IsNotSpace(SYSTEM_AREACODE_VALUE) &amp;&amp; SYSTEM_DUOGUI_AREACODE_VALUE == SYSTEM_AREACODE_VALUE){
				$params.isChangShaShiData = true;
				
				//将具有&quot;多规合一&quot;数据的机构查出
				$.ajax({
					&quot;url&quot; : c2.base(&quot;service&quot;)+&quot;dzjc_tongji/v1/getDuoGuiOrg&quot;,
					&quot;type&quot; : &quot;get&quot;,
					&quot;dataType&quot;: &quot;json&quot;,
					&quot;success&quot; : function(data){
						$params.duoGuiOrgMap = data;
					},
					&quot;error&quot;:function(data){
						Messenger.error({type:'error',message:'多规合一机构查询失败！'});
					}
				});
			}else{
				$params.isChangShaShiData = false;
			}
		}else{
			Messenger.error({type:'error',message:'多规合一参数SYSTEM_AREACODE_VALUE获取失败'});
		}
	});
},
//查询事件
button_click:function($event){
	functions.loadData();
},
//Excel导出
button2_click:function($event){
	Modal.openConfirm({message:&quot;是否导出所有数据！&quot;},function(data){
	//导出调用查询方法
	functions.button_click();
	var beginDateLong = new Date($view.dateTimeRangePicker.getStartDatetimeObj().getValue()).getTime();
	var endDateLong = new Date($view.dateTimeRangePicker.getEndDatetimeObj().getValue()).getTime();
	var url = c2.base(&quot;service&quot;)+&quot;JCSumSuperviseInfoAllServlet?type=12&amp;beginDate=&quot;+beginDateLong+&quot;&amp;endDate=&quot;+endDateLong+&quot;&amp;org_id=&quot;+$params.orgId;
			window.location.href =url;
	});
},
//word导出
button3_click:function($event){
	Modal.openConfirm({message:&quot;是否导出所有数据！&quot;},function(data){
	//导出调用查询方法
	functions.button_click();
	var beginDateLong = new Date($view.dateTimeRangePicker.getStartDatetimeObj().getValue()).getTime();
	var endDateLong = new Date($view.dateTimeRangePicker.getEndDatetimeObj().getValue()).getTime();
	var url = c2.base(&quot;service&quot;)+&quot;JCSumSuperviseInfoAllServlet?type=11&amp;beginDate=&quot;+beginDateLong+&quot;&amp;endDate=&quot;+endDateLong+&quot;&amp;org_id=&quot;+$params.orgId;
			window.location.href =url;
	});
},

dateTimeRangePicker_startOnChange:function(date,oldDate){
	$params.beginDate = date;
},

dateTimeRangePicker_endOnChange:function(date,oldDate){
	$params.endDate = date;
},

page_afterDataSourceLoad:function(){
},

//点击可展示&quot;多规合一&quot;数据
grid_areaOrgName_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
		
	//1,定义所需数据
	var html = &quot;&quot;;
	var rowid = rowObject.orgId;
	var orgLevel = rowObject.orgLevelInt;
	var areaOrgCode = rowObject.areaOrgCode;
	//2,向点击行之后插入&quot;多规合一&quot;数据(非长沙市用户登录，无此功能)
	//countryCode及orgLevel符合要求，为长沙市直下机构，才存在多规数据
	if(rowid != null &amp;&amp; areaOrgCode != null &amp;&amp; orgLevel != null 
			&amp;&amp; $params.isChangShaShiData &amp;&amp; orgLevel == '5' &amp;&amp; areaOrgCode.substring(0,12) == '430101000000' ){
		html = &quot;&lt;font style='cursor: pointer;' ng-click='functions.duoGuiParams(rowObject);'>&quot;+cellvalue+&quot;&lt;/font> &quot;;

	}else{
		html = cellvalue
	}
	return html;
},
duoGuiParams:function(rowObject){
	
	//0,判断该机构是否有&quot;多规合一&quot;数据
	if($params.duoGuiOrgMap[rowObject.orgId] == null){
		return;
	}
	//1,当前表格的id
	var tableId = 'list';
	//2,是否是&quot;多规合一&quot;数据(1,是;0:不是)
	var isInvestmentSupervision = &quot;1&quot;;
	var beginDate = new Date($params.beginDate).getTime();
	var endDate = new Date($params.endDate).getTime();
	var baseUrl = c2.base(&quot;service&quot;)+&quot;dzjc_tongji/v1/getByBanJianTongJiOrg&quot;;
	var rowid = rowObject.orgId;
	//3,插入&quot;多规合一&quot;数据
	insertDuoGuiRow(tableId,rowid,beginDate,endDate,isInvestmentSupervision,baseUrl);
}</form:scripts>
  <form:htmls>
&lt;script type=&quot;text/ng-template&quot; id=&quot;banjiantongji_div2_html&quot;>
&lt;center>&lt;h3 ng-model='breadcrumbArray'>{{breadcrumbArray[1]}}&lt;/h3>&lt;/center>&lt;br/>
&lt;/script>
&lt;script type=&quot;text/ng-template&quot; id=&quot;div1_html&quot;>
	&lt;h6 class=&quot;hhh1&quot;>温馨提示：&lt;/h6>
	&lt;h6 class=&quot;hhh2&quot;>
		法定提速率 = (所有办件法定时限总和 - 所有办件所用时长) / 所有办件法定时限总和（所有办件所用时长：所有办件受理到办结时长；所有办件法定时限总和：包含法定时限和特别程序时限）&lt;br/>
	&lt;/h6>
	&lt;h6 class=&quot;hhh2&quot;>
		承诺提速率 = (所有办件承诺时限总和 - 所有办件所用时长) / 所有办件承诺时限总和（所有办件所用时长：所有办件受理到办结时长；所有办件承诺时限总和：包含承诺时限和特别程序时限）
	&lt;/h6>
	&lt;h6 class=&quot;hhh2&quot;>
		如果办结率>100%，则上一时间段收到办件在查询时间段内进行办结，导致办结数>收件数。
	&lt;/h6>
&lt;/script></form:htmls>
</form:Form>
