  <form:datasources>
    <form:datasource xsi:type="form:PagingDataSource" id="list" name="列表数据源" base="service" sn="dzjc_tongji/v1/ProcedureStatusDetail" elementType="entity:com.chinacreator.dzjc_tongji.ProcedureStatusDetail">
      <form:entityObj pk="id" restResource="true"/>
