  <form:scripts>

button_click:function($event){
	if(!angular.isUndefined($model.entity.appealContent)){
		$model.entity.id=$params.uuid;
		$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_dealtMatter/v1/taJcWarningAppeal&quot;,$model.entity).success(function(data){
			if(data.id){
				Modal.instance.close();
				Messenger.post({type:'success',message:'提交成功'});
			}else{
				Messenger.error(&quot;提交失败&quot;);
			}
		});
		/*
		$model.entity.$save(function(){
			Modal.instance.close();
			Messenger.post({type:'success',message:'新增成功'});
		});
		$model.entity.$save(function(data){
			//$view.attach.submit({complainId:data.id,businessType:&quot;2&quot;});
			Modal.instance.close();
			Messenger.post({type:'success',message:'保存成功'});
		});*/
	}else{
		Messenger.error(&quot;请填写申诉内容&quot;);
	}
},

page_onload:function(){	
	var orgId=c2.subject.orgIds[0];
	$http.get(c2.base(&quot;service&quot;)+&quot;dzjc_complain/v1/orgview/&quot;+orgId).success(function(data){
		$model.entity.appealOrgname=data.name;
	});
	$model.entity.appealOrgid=c2.subject.orgIds[0];
	$model.entity.appealUserName=c2.subject.realname;
	$model.entity.appealTime=new Date().Format(&quot;yyyy-MM-dd hh:mm:ss&quot;);
	$model.entity.superviseInfoId=$params.superviseHumanId;
},

button1_click:function($event){
	Modal.instance.close();
},

attachinfo_button_click:function($event){
	if(!IsSpace($model.entity.attachname)){
		$view.attach.submit({complainId:$params.uuid,businessType:&quot;2&quot;,attachname:$model.entity.attachname});
	}else{
		Messenger.post({type:'error',message:'请输入材料名称'});
	}
	
},
createTime_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	return moment(cellvalue).format(&quot;YYYY-MM-DD&quot;);
},

multipleFileInputIframe_onSuccess:function(data){
	//$view.attach.submit({complainId:$params.uuid,businessType:&quot;2&quot;});
	$view.attachList.refresh({&quot;businessType&quot;:&quot;2&quot;,&quot;complainId&quot;:$params.uuid});
	$view.attachname.clean();
	$view.attach.reset();
},

page_beforeDataSourceLoad:function($params){
	$params.uuid=uuid();
},

attachList_beforeInit:function(jqGridOptions){
	
},

multipleFileInputIframe_onError:function(data){
	Messenger.post({type:'error',message:'上传失败'});
},

attachName_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	return cellvalue.substr(cellvalue.indexOf(&quot;$$&quot;)+2);
},

colJqgrid_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	var htmls = 
				//&quot;&lt;a style='cursor: pointer;' ng-click='functions.attach_download(rowObject);'>下载&lt;/a> &amp;nbsp;&amp;nbsp;&quot;
			  &quot;&lt;a style='cursor: pointer;' ng-click='functions.attach_delete(rowObject);'>删除&lt;/a>&quot;;
	return htmls;
},
attach_preview:function(rowObject){
	
},
attach_download:function(rowObject){
	var url =&quot;/dzjc/ComplainAttachinfoServlet?attachId=&quot;+rowObject.attachId+&quot;&amp;fileName=&quot;+encodeURIComponent(encodeURIComponent(rowObject.attachName));
	window.location.href =url;
},
attach_delete:function(rowObject){
	Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},function(data){
		$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_complain/v1/taJcComplainAttachinfo/&quot;+rowObject.attachId).success(function(){
			$view.attachList.refresh({&quot;businessType&quot;:&quot;2&quot;,&quot;complainId&quot;:$params.uuid});
		});
	});
},</form:scripts>
