clean_button_click:function($event){
	$view.mainForm.clean();
	$view.list.refresh({});
},

//双击行，弹出受理窗口
list_ondblClickRow:function(rowid,iRow,iCol,e){

	Modal.open(&quot;f/dzjc_complain/taJcComplainQueryInfo&quot;,{'complainId':rowid},function(data){
		if(data != 'isClose'){
			$view.mainForm.clean();
			$view.list.refresh({});
		}
	});
},

proessstate_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(!angular.isUndefined(cellvalue)){
		if(cellvalue == '0'){
			cellvalue = '预受理';
		}
		else if(cellvalue == '1'){
			cellvalue = '待受理';
		}
		else if(cellvalue == '3'){
			cellvalue = '不予受理';
		}
		else if(cellvalue == '4'){
			cellvalue = '待接收';
		}
		else if(cellvalue == '5'){
			cellvalue = '待回复';
		}
		else if(cellvalue == '6'){
			cellvalue = '区县级待审核';
		}
		else if(cellvalue == '7'){
			cellvalue = '市级待审核';
		}
		else if(cellvalue == '8'){
			cellvalue = '省级待审核';
		}
		else if(cellvalue == '9'){
			cellvalue = '办理完成';
		}
		else {
			cellvalue = '';
		}
		return cellvalue;
	}
}</form:scripts>
</form:Form>
