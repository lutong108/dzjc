  <form:scripts>//自定义查询
query_button_click:function($event){

	var queryParams = new Array();

	if($model.list.complainTitle!= '' &amp;&amp; $model.list.complainTitle!= null){
		queryParams.push({field:&quot;complainTitle&quot;,op:&quot;cn&quot;,data:$model.list.complainTitle}) ;
	}
	if($model.list.complainUserName!= '' &amp;&amp; $model.list.complainUserName!= null){
		queryParams.push({field:&quot;complainUserName&quot;,op:&quot;eq&quot;,data:$model.list.complainUserName}) ;
	}
	if($model.list.queryCode!= '' &amp;&amp; $model.list.queryCode!= null){
		queryParams.push({field:&quot;queryCode&quot;,op:&quot;eq&quot;,data:$model.list.queryCode}) ;
	}
	if($model.list.proessstate!= '' &amp;&amp; $model.list.proessstate){
		queryParams.push({field:&quot;proessstate&quot;,op:&quot;eq&quot;,data:$model.list.proessstate}) ;
	}
	if(beginDate.value!= '' &amp;&amp; beginDate.value!= null){
		queryParams.push({field:&quot;enregisterTime&quot;,op:&quot;ge&quot;,data:beginDate.value}) ;
	}
	if(endDate.value!= '' &amp;&amp; endDate.value!= null){
	
		if(beginDate.value > endDate.value){
			Messenger.error(&quot;开始时间不能大于结束时间！&quot;);
			return;
		}
	
		queryParams.push({field:&quot;enregisterTime&quot;,op:&quot;le&quot;,data:endDate.value}) ;
	}
	
	if(queryParams.length != 0 ){
		var filters={groupOp:&quot;AND&quot;,rules:queryParams}
		$view.list.refresh({filters:filters});
	}else{
		$view.list.refresh({filters:filters});
	}
},

