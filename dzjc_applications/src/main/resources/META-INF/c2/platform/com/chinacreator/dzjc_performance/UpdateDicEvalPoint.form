  <form:scripts>
btn_update_click:function($event){
	var checked = $view.mainForm.check();
	if(!checked){
		Messenger.error('请填写完整信息');
	}
	if(Number($model.entity.maxValue) > 100){
		Messenger.error('最大值不能大于100');
	}
	if(Number($model.entity.minValue) &lt; 0){
		Messenger.error('最小值不能小于0');
	}
	console.log(Number($model.entity.maxValue));
	if(''+Number($model.entity.maxValue)=='NaN'||&quot;&quot;+Number($model.entity.minValue)=='NaN'){
		Messenger.error('最大值或最小值包含非法字符');
	}
	console.log(typeof(Number($model.entity.maxValue)));
	if(checked &amp;&amp; Number($model.entity.maxValue) &lt;= 100 &amp;&amp; Number($model.entity.minValue) >= 0){
		$model.entity.$save(function(){
			Modal.instance.close();
			Messenger.post({type:'success',message:'修改成功'});
		});
	}
},

btn_edit_click:function($event){
	Modal.instance.close();
},

raidxName_nodeClick:function(event,treeId,treeNode,clickFlag){
	if($model.entity.evalFormula == undefined ||$model.entity.evalFormula == null){
		$model.entity.evalFormula ='';
	}
	$model.entity.evalFormula += treeNode.name;
	console.log($model.entity.evalFormula);
}</form:scripts>
