<?xml version="1.0" encoding="UTF-8"?>
<form:Form xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dync="http://www.chinacreator.com/platform/mvc/ui/dync" xmlns:form="http://www.chinacreator.com/platform/mvc/form" name="发牌数据统计" sn="dzjc_ruleEngine/SpStatusList" type="form">
  <form:control type="page/normal">
    <form:attributes source="page/normal">
      <dync:attribute default="" group="HTML" id="showHead" name="显示标题" value="">
        <dync:render/>
        <dync:children/>
      </dync:attribute>
      <dync:attribute default="" group="HTML" id="title" name="标题" value="">
        <dync:render/>
        <dync:children/>
      </dync:attribute>
      <dync:attribute default="" group="HTML" id="desc" name="描述" value="">
        <dync:render/>
        <dync:children/>
      </dync:attribute>
      <dync:attribute default="false" group="HTML" id="fixedHeight" name="自动高度" value="false">
        <dync:render/>
        <dync:children/>
      </dync:attribute>
      <dync:attribute default="false" group="HTML" id="compact" name="紧凑页面" value="false">
        <dync:render/>
        <dync:children/>
      </dync:attribute>
      <dync:attribute default="false" group="HTML" id="dirtyCheck" name="脏检查" value="false">
        <dync:render/>
        <dync:children/>
      </dync:attribute>
      <dync:attribute default="" group="注入配置" id="angularJsServices" name="注入服务" value="">
        <dync:render/>
        <dync:children/>
      </dync:attribute>
      <dync:attribute default="" group="事件" id="beforeDataSourceLoad" name="数据源加载前" value="">
        <dync:render/>
        <dync:children/>
      </dync:attribute>
      <dync:attribute default="" group="事件" id="afterDataSourceLoad" name="数据源加载后" value="">
        <dync:render/>
        <dync:children/>
      </dync:attribute>
      <dync:attribute default="" group="事件" id="onload" name="页面加载完成" value="">
        <dync:render/>
        <dync:children/>
      </dync:attribute>
    </form:attributes>
  </form:control>
  <form:child xsi:type="form:Group" id="divTop" name="头部">
    <form:control type="container/div">
      <form:attributes source="container/div">
        <dync:attribute default="" group="高级自定义" id="style" name="自定义style" value="height:30px;">
          <dync:render colSpan="2" height="0" multi="false" name="text"/>
          <dync:children/>
        </dync:attribute>
      </form:attributes>
    </form:control>
  </form:child>
  <form:child xsi:type="form:Group" id="divSearch" name="搜索条件">
    <form:control type="container/div">
      <form:attributes source="container/div"/>
    </form:control>
    <form:child xsi:type="form:Group" id="div5" name="简单容器(DIV)">
      <form:control type="container/div">
        <form:attributes source="container/div">
          <dync:attribute default="false" group="HTML配置" id="nopadding" name="无内边距" value="true">
            <dync:render colSpan="2" height="0" multi="false" name="checkbox"/>
            <dync:children/>
          </dync:attribute>
          <dync:attribute default="0" group="HTML配置" id="offset" name="偏移" value="">
            <dync:render colSpan="1" height="0" multi="false" name="text"/>
            <dync:children/>
          </dync:attribute>
        </form:attributes>
      </form:control>
      <form:child xsi:type="form:Field" binding="$params.startDate" id="startDate" name="监管开始日期:" customize="true">
        <form:control type="widget/date_picker">
          <form:attributes source="widget/date_picker">
            <dync:attribute default="75" group="基本设置" id="lableWidth" name="文字宽度" value="95">
              <dync:render colSpan="1" height="0" multi="false" name="text"/>
              <dync:children/>
            </dync:attribute>
          </form:attributes>
        </form:control>
        <form:validation/>
      </form:child>
      <form:child xsi:type="form:Field" binding="$params.endDate" id="endDate" name="监管结束日期:" customize="true">
        <form:control type="widget/date_picker">
          <form:attributes source="widget/date_picker">
            <dync:attribute default="75" group="基本设置" id="lableWidth" name="文字宽度" value="95">
              <dync:render colSpan="1" height="0" multi="false" name="text"/>
              <dync:children/>
            </dync:attribute>
          </form:attributes>
        </form:control>
        <form:validation/>
      </form:child>
    </form:child>
    <form:child xsi:type="form:Group" id="div5" name="简单容器(DIV)">
      <form:control type="container/div">
        <form:attributes source="container/div">
          <dync:attribute default="false" group="HTML配置" id="nopadding" name="无内边距" value="true">
            <dync:render colSpan="2" height="0" multi="false" name="checkbox"/>
            <dync:children/>
          </dync:attribute>
          <dync:attribute default="" group="高级自定义" id="style" name="自定义style" value="text-align: center; ">
            <dync:render colSpan="2" height="0" multi="false" name="text"/>
            <dync:children/>
          </dync:attribute>
        </form:attributes>
      </form:control>
      <form:child xsi:type="form:Field" id="searchButton" name="查询" customize="true">
        <form:control type="widget/button">
          <form:attributes source="widget/button">
            <dync:attribute default="" group="事件设置" id="click" name="点击事件" value="searchButton_click($event)">
              <dync:render colSpan="1" height="0" multi="false" name="event">
                <dync:item name="params" value="$event"/>
              </dync:render>
              <dync:children/>
            </dync:attribute>
          </form:attributes>
        </form:control>
        <form:validation/>
      </form:child>
      <form:child xsi:type="form:Field" id="resetButton" name="重置" customize="true">
        <form:control type="widget/button">
          <form:attributes source="widget/button">
            <dync:attribute default="" group="事件设置" id="click" name="点击事件" value="resetButton_click($event)">
              <dync:render colSpan="1" height="0" multi="false" name="event">
                <dync:item name="params" value="$event"/>
              </dync:render>
              <dync:children/>
            </dync:attribute>
          </form:attributes>
        </form:control>
        <form:validation/>
      </form:child>
    </form:child>
  </form:child>
  <form:child xsi:type="form:Group" binding="$model.list" id="list" name="发牌执行结果列表">
    <form:control type="container/table_jqgrid">
      <form:attributes source="container/table_jqgrid">
        <dync:attribute default="true" group="表格设置" id="multiselect" name="多选" value="false">
          <dync:render colSpan="1" height="0" multi="false" name="checkbox"/>
          <dync:children/>
        </dync:attribute>
        <dync:attribute default="" group="加载后事件" id="ondblClickRow" name="双击行" value="list_ondblClickRow(rowid,iRow,iCol,e)">
          <dync:render colSpan="1" height="0" multi="false" name="event">
            <dync:item name="param" value="rowid,iRow,iCol,e"/>
          </dync:render>
          <dync:children/>
        </dync:attribute>
      </form:attributes>
    </form:control>
    <form:child xsi:type="form:Group" id="toolbar_table" name="toolbar_table">
      <form:control type="container/toolbar_table">
        <form:attributes source="container/toolbar_table"/>
      </form:control>
    </form:child>
    <form:child xsi:type="form:Field" binding="batchId" id="batchId" name="主键ID" customize="true">
      <form:control type="widget/col_jqgrid">
        <form:attributes source="widget/col_jqgrid">
          <dync:attribute default="false" group="表格设置" id="key" name="主键列" value="true">
            <dync:render colSpan="1" height="0" multi="false" name="checkbox"/>
            <dync:children/>
          </dync:attribute>
          <dync:attribute default="false" group="表格设置" id="hidden" name="隐藏列" value="true">
            <dync:render colSpan="1" height="0" multi="false" name="checkbox"/>
            <dync:children/>
          </dync:attribute>
        </form:attributes>
      </form:control>
      <form:validation/>
    </form:child>
    <form:child xsi:type="form:Field" binding="superviseTime" id="superviseTime" name="监管时间" customize="true">
      <form:control type="widget/col_jqgrid">
        <form:attributes source="widget/col_jqgrid">
          <dync:attribute default="" group="格式设置" id="formatoptions" name="预定格式设置" value="{dateFormat:&quot;yyyy-MM-dd HH:mm:ss&quot;}">
            <dync:render colSpan="1" height="0" multi="false" name="text"/>
            <dync:children/>
          </dync:attribute>
          <dync:attribute default="" group="格式设置" id="formatter" name="预定格式" value="millisecondDate">
            <dync:render colSpan="1" height="0" multi="false" name="select">
              <dync:item name="空" value=""/>
              <dync:item name="整数" value="integer"/>
              <dync:item name="数字" value="number"/>
              <dync:item name="货币" value="currency"/>
              <dync:item name="毫秒日期" value="millisecondDate"/>
              <dync:item name="邮件" value="email"/>
              <dync:item name="链接" value="link"/>
              <dync:item name="带参链接" value="showlink"/>
              <dync:item name="勾选框" value="checkbox"/>
              <dync:item name="选择" value="select"/>
              <dync:item name="事件" value="actions"/>
              <dync:item name="数据字典" value="dictionary"/>
            </dync:render>
            <dync:children/>
          </dync:attribute>
          <dync:attribute default="150" group="基本设置" id="width" name="宽度" value="200">
            <dync:render colSpan="1" height="0" multi="false" name="text"/>
            <dync:children/>
          </dync:attribute>
        </form:attributes>
      </form:control>
      <form:validation/>
    </form:child>
    <form:child xsi:type="form:Field" binding="superviseTotal" id="superviseTotal" name="监管总数" customize="true">
      <form:control type="widget/col_jqgrid">
        <form:attributes source="widget/col_jqgrid"/>
      </form:control>
      <form:validation/>
    </form:child>
    <form:child xsi:type="form:Field" binding="runCount" id="runCount" name="线程失败次数" customize="true">
      <form:control type="widget/col_jqgrid">
        <form:attributes source="widget/col_jqgrid">
          <dync:attribute default="150" group="基本设置" id="width" name="宽度" value="120">
            <dync:render colSpan="1" height="0" multi="false" name="text"/>
            <dync:children/>
          </dync:attribute>
        </form:attributes>
      </form:control>
      <form:validation/>
    </form:child>
    <form:child xsi:type="form:Field" binding="failTotal" id="failTotal" name="失败数" customize="true">
      <form:control type="widget/col_jqgrid">
        <form:attributes source="widget/col_jqgrid"/>
      </form:control>
      <form:validation/>
    </form:child>
  </form:child>
  <form:datasources>
    <form:datasource xsi:type="form:PagingDataSource" id="list" name="列表型数据源" base="service" pageSize="10" type="url" url="dzjc_ruleEngine/v1/spstatus/getListByPage"/>
  </form:datasources>
  <form:scripts>searchButton_click:function($event){
	var queryParams = new Array();
	var queryParamsDc = [];
  
	if($params.startDate!= '' &amp;&amp; $params.startDate!= null){
	    queryParams.push({field:&quot;superviseTime&quot;,op:&quot;ge&quot;,data:$params.startDate}) ;
	    queryParamsDc.push({field:&quot;superviseTime&quot;,op:&quot;ge&quot;,data:$params.startDate}) ;
	}
	if($params.endDate!= '' &amp;&amp; $params.endDate!= null){
	    if($params.startDate > $params.endDate){
	    	Messenger.error(&quot;开始时间不能大于结束时间！&quot;);
	    	return;
	    }
	    queryParams.push({field:&quot;superviseTime&quot;,op:&quot;le&quot;,data:$params.endDate}) ;
	    queryParamsDc.push({field:&quot;superviseTime&quot;,op:&quot;le&quot;,data:$params.endDate}) ;
	}
	if(queryParams.length != 0 ){
		var filters={groupOp:&quot;AND&quot;,rules:queryParams}
		var filtersDc={
				groupOp:&quot;AND&quot;,
				rules:queryParamsDc
		}
		$params.cond = {
				filters:filtersDc
		}
		$view.list.refresh({filters:filters});
	}else{
		$params.cond = {
				filters:filters
		}
		$view.list.refresh({filters:filters});
	}
},

resetButton_click:function($event){
	$view.mainForm.clean();
	$view.list.refresh({filters:null});
},

list_ondblClickRow:function(rowid,iRow,iCol,e){
	console.log(rowid);
	if(rowid){
        Modal.open(&quot;f/dzjc_ruleEngine/SpStatusInfo&quot;,{batchId:rowid},function(data){
          $view.list.refresh();
        });
  	}else{
  		Messenger.error(&quot;请选中表格项!&quot;);
  	}
}</form:scripts>
</form:Form>
