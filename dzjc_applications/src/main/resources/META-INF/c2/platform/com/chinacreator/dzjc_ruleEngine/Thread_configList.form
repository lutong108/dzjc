  <form:scripts>buttonAdd_click:function(rowId,rowData,tableData){&#xD;
	Modal.open(&quot;f/entity/createThread_config&quot;,{},function(data){&#xD;
		&#xD;
		$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/selectAllList&quot;).success(function(data){&#xD;
		if(data==&quot;1&quot;){&#xD;
		$(&quot;#buttonAdd&quot;).hide();&#xD;
		}&#xD;
		});&#xD;
		$view.list.refresh();&#xD;
	});&#xD;
},&#xD;
buttonEdit_click:function(rowId,rowData,tableData){&#xD;
	if(rowId){&#xD;
		Modal.open(&quot;f/entity/updateThread_config&quot;,{id:rowId},function(data){&#xD;
			$view.list.refresh();&#xD;
		});&#xD;
	}else{&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}&#xD;
},&#xD;
buttonDelete_click:function(rowId,rowData,tableData){&#xD;
	var ids = $view.list.getSelectRowIds() ;&#xD;
	if(ids.length==0){&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}else{&#xD;
		Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},&#xD;
			function(data){&#xD;
				if(ids.length==1){&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/thread_config/&quot;+ids[0]).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				}else{&#xD;
					 var queryStr=&quot;&quot;;&#xD;
					 for (var i =0; i&lt; ids.length;i++){&#xD;
						if(i==0){&#xD;
						   queryStr+=&quot;id=&quot;+ids[i] ;&#xD;
						}else{&#xD;
						   queryStr+=&quot;&amp;id=&quot;+ids[i];&#xD;
						}&#xD;
					  }&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/thread_config?&quot;+queryStr).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				 }&#xD;
		});&#xD;
	}&#xD;
},

list_afterInit:function(events){
	$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/selectAllList&quot;).success(function(data){&#xD;
		if(data==&quot;1&quot;){&#xD;
		$(&quot;#buttonAdd&quot;).hide();&#xD;
		}&#xD;
		&#xD;
	});
},

list_ondblClickRow:function(rowid,iRow,iCol,e){
	if(rowid){&#xD;
		Modal.open(&quot;f/entity/updateThread_config&quot;,{id:rowid},function(data){&#xD;
			$view.list.refresh();&#xD;
		});&#xD;
	}else{&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}
}</form:scripts>
