button_click:function($event){&#xD;
debugger;
	var superviseResultNo= $model.list.superviseResultNo;&#xD;
	var approveName= $model.list.approveName;&#xD;
	var state= $model.list.state;&#xD;
	var fpbeginDate= $params.fpbeginDate;&#xD;
