  <form:scripts>//佐证材料上传
attachinfo_button_click:function($event){
	$view.multipleFileInputIframe.submit({complainId:$model.entity.complainId});
},

//佐证材料上传成功
multipleFileInputIframe_onSuccess:function(data){
	$view.multipleFileInputIframe.reset();
	Messenger.post({type:'success',message:'上传成功'});
	
	$view.taJcComplainAttachinfolist.refresh({&quot;complainId&quot;:$model.entity.complainId});
},

//佐证材料上传失败
multipleFileInputIframe_onError:function(data){
	if(data != null &amp;&amp; data != &quot;&quot;){
		Messenger.post({type:'error',message:data.errorMsg});
	}else{
		Messenger.post({type:'error',message:'上传失败'});
	}
	
	$view.taJcComplainAttachinfolist.refresh({&quot;complainId&quot;:$model.entity.complainId});
},

//正式回复扫描件上传
attachinfo_button_click2:function($event){
	$view.multipleFileInputIframe2.submit({complainId:$model.entity.complainId+&quot;_zhengshi&quot;});
},

//正式回复扫描件上传成功
multipleFileInputIframe_onSuccess2:function(data){
	$view.multipleFileInputIframe2.reset();
	Messenger.post({type:'success',message:'上传成功'});
	
	$view.taJcComplainAttachinfolist2.refresh({&quot;complainId&quot;:$model.entity.complainId+&quot;_zhengshi&quot;});
},

//正式回复扫描件上传失败
multipleFileInputIframe_onError2:function(data){
	if(data != null &amp;&amp; data != &quot;&quot;){
		Messenger.post({type:'error',message:data.errorMsg});
	}else{
		Messenger.post({type:'error',message:'上传失败'});
	}
	
	$view.taJcComplainAttachinfolist2.refresh({&quot;complainId&quot;:$model.entity.complainId+&quot;_zhengshi&quot;});
},

//数据来源样式
datasource_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(cellvalue=='1'){
		return &quot;外网&quot;;
	}else{
		return &quot;内网&quot;;
	}
},

//上传时间样式
createTime_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	var date = new Date(cellvalue);
	var commonTime = date.toLocaleString('chinese',{hour12:false});
	return commonTime;
},

//材料名称样式
attachName_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	return cellvalue.substring(cellvalue.lastIndexOf(&quot;$$&quot;)+2);
},

//佐证材料/正式扫描件操作栏
colJqgrid_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	
	var htmls = &quot;&lt;a style='cursor: pointer;' ng-click='functions.attach_download(rowObject);'>下载&lt;/a>&quot;;
			  
	return htmls;
},
//正式操作栏
colJqgrid2_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	var htmls = &quot;&lt;a style='cursor: pointer;' ng-click='functions.attach_download(rowObject);'>下载&lt;/a> &amp;nbsp;&amp;nbsp;&quot;
		+ &quot;&lt;a style='cursor: pointer;' ng-click='functions.attach_delete(rowObject);'>删除&lt;/a>&quot;;
	
	return htmls;
},
//删除材料信息
attach_delete:function(rowObject){

	Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},function(data){
		$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_complain/v1/taJcComplainAttachinfo/&quot;+rowObject.attachId).success(function(){
			//$view.taJcComplainAttachinfolist.refresh({&quot;complainId&quot;:$model.entity.complainId});
			$view.taJcComplainAttachinfolist2.refresh({&quot;complainId&quot;:$model.entity.complainId+&quot;_zhengshi&quot;});
		});
	});
},

//下载材料信息
attach_download:function(rowObject){
	var url =&quot;/dzjc/ComplainAttachinfoServlet?attachId=&quot;+rowObject.attachId+&quot;&amp;fileName=&quot;+encodeURIComponent(encodeURIComponent(rowObject.attachName));
	window.location.href =url;
},

//数据源加载后
page_afterDataSourceLoad:function(){
	if($model.entity.isSatisfied == &quot;满意&quot; || $model.entity.isSatisfied == &quot;比较满意&quot;){
		$(&quot;#button&quot;).attr(&quot;disabled&quot;,&quot;true&quot;);//添加disabled属性 
	}
	//获取当前登录用户信息
	$http.get(c2.base(&quot;service&quot;)+&quot;dzjc_complain/v1/taJcComplainBaseinfo/getUserInfo&quot;).success(function(data){
		//判断是否是最后一级审核，如果当前登录人的机构层级=接收单位层级，则为最后一级，
		//如果是最后一级审核，显示出结果是否公开的选项
		if(data != null &amp;&amp; data.orgLevel != null){
			if (data.orgLevel== $model.entity.acceptOrgLevel){
				$(&quot;#div3&quot;).show();
				$model.isLastAudit = true;
			}
			else {
				$(&quot;#div3&quot;).hide();
				$model.isLastAudit = false;
			}
		}
		//判断是否是自办，如果接收单位=当前审核单位，则认为是自办
		//如果是自办，不显示退回按钮
		if(data != null &amp;&amp; data.orgId != null){
			if (data.orgId==$model.entity.acceptOrgId){
				$(&quot;#button3&quot;).hide();
			}
			else {
				$(&quot;#button3&quot;).show();
			}
		}
	});
	
	if($model.entity.expireDate !=null &amp;&amp; $model.entity.expireDate !=&quot;&quot;){
		var date = new Date($model.entity.expireDate);
		var commonTime = date.toLocaleString('chinese',{hour12:false});
		$model.entity.expireDate = commonTime;
	}
},

//返回
button6_click:function($event){
	Modal.instance.close('isClose');
},

//查看意见
button1_click:function($event){
	Modal.open(&quot;f/dzjc_complain/taJcComplainTasklist&quot;,{'complainId':$model.entity.complainId});
},

//提交
button2_click:function($event){
	$view.button2.setDisabled(true);
	if(!handleIdea.value){
		Messenger.post({type:'error',message:'处理意见不能为空'});
		$(&quot;#handleIdea&quot;).focus();
		$view.button2.setDisabled(false);
		return;
	}
	
	if(handleIdea.value.length > 500){
		Messenger.post({type:'error',message:'处理意见不能超过500字符'});
		$(&quot;#handleIdea&quot;).focus();
		$view.button2.setDisabled(false);
		return;
	}
	
	var handleisPublic = 'N';
	if($model.isLastAudit){
		if(!$model.entity.handleisPublic){
			Messenger.post({type:'error',message:'处理结果不能为空'});
			$(&quot;#handleisPublic&quot;).focus();
			$view.button2.setDisabled(false);
			return;
		}
		else {
			handleisPublic = $model.entity.handleisPublic;
		}
	}
	
	Modal.openConfirm({message:&quot;确认审核通过&quot;},function(){
		var map ={
			&quot;complainId&quot;:$model.entity.complainId,
			&quot;handleIdea&quot;:handleIdea.value.trim(),
			&quot;pass&quot;:&quot;Y&quot;,
			&quot;handleisPublic&quot;:handleisPublic
		};
		
		$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_complain/v1/taJcComplainBaseinfo/updateSh&quot;,map).success(function(data){
			if(data!=null &amp;&amp; data!=&quot;&quot;){
				Modal.instance.close();
				Messenger.post({type:'success',message:'提交成功'});
			}else{
				Messenger.post({type:'error',message:'请先上传已盖章的正式扫描件'});
			}
		});
	},function(){
		$view.button2.setDisabled(false);
	});
	$view.button2.setDisabled(false);  
},

//退回
button3_click:function($event){
	$view.button3.setDisabled(true);
	if(!handleIdea.value){
		Messenger.post({type:'error',message:'处理意见不能为空'});
		$(&quot;#handleIdea&quot;).focus();
		$view.button3.setDisabled(false);
		return;
	}
	
	if(handleIdea.value.length > 500){
		Messenger.post({type:'error',message:'处理意见不能超过500字符'});
		$(&quot;#handleIdea&quot;).focus();
		$view.button3.setDisabled(false);
		return;
	}
	
	

	Modal.openConfirm({message:&quot;确认审核不通过&quot;},function(){
		var map ={
			&quot;complainId&quot;:$model.entity.complainId,
			&quot;handleIdea&quot;:handleIdea.value.trim(),
			&quot;pass&quot;:&quot;N&quot;
	};
	
	$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_complain/v1/taJcComplainBaseinfo/updateSh&quot;,map).success(function(data){
		if(data!=null &amp;&amp; data!=&quot;&quot;){
			Modal.instance.close();
			Messenger.post({type:'success',message:'退回成功'});
		}else{
			Messenger.post({type:'error',message:'请先上传已盖章的正式扫描件'});
		}
	});
	},function(){
		$view.button3.setDisabled(false);
	});
	$view.button3.setDisabled(false);
},

button5_click:function($event){
	var printMap={};
	var queryMap=new Object();
	queryMap.complainId=$model.entity.complainId;
	queryMap.tacheName=&quot;受理审核&quot;;
	$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_complain/v1/getSingleTask&quot;,queryMap).success(function(data){
		printMap.handleIdeaSL=data.handleIdea;
		printMap.acceptTime=moment().format(&quot;YYYY-MM-DD&quot;);
		printMap.caseno=$model.entity.caseno;
		printMap.complainUserName=$model.entity.complainUserName;
		printMap.complainUserPhone=$model.entity.complainUserPhone;
		printMap.complainUserAddress=$model.entity.complainUserAddress;
		printMap.bycomplainedOrg=$model.entity.bycomplainedOrg;
		printMap.bycomplainedUserOrg=$model.entity.bycomplainedUserOrg;
		printMap.bycomplainedUserPhone=$model.entity.bycomplainedUserPhone;
		printMap.appeptUser=c2.subject.realname;
		printMap.appeptUserFz=c2.subject.realname;
		printMap.complainContent=$model.entity.complainContent;
		printMap.mainappeal=$model.entity.mainappeal;
		if(!IsSpace(handleIdea.value)){
			printMap.handleIdeaSH=handleIdea.value;
		}else{
			Messenger.post({type:'error',message:'请填写处理意见'});
			return ;
		}
		var message=JSON.stringify(printMap);
		$(&quot;#message&quot;).val(message);
		var printIndex = layer.msg('正在打开打印预览页面，请耐心等待...', {
			  icon: 16,
			  shade: 0.2,
			  time: 0
		});
		var url=&quot;jsp/printComplain.jsp?message=&quot;+message;
		$(&quot;#hidIframe&quot;).attr('src',url);
	});
}</form:scripts>
  <form:htmls>
&lt;script type=&quot;text/ng-template&quot; id=&quot;div4_html&quot;>
	&lt;iframe id=&quot;hidIframe&quot; name=&quot;hidIframe&quot; width=&quot;0&quot; height=&quot;0&quot;  frameborder=&quot;no&quot; border=&quot;0&quot; marginwidth=&quot;0&quot; marginheight=&quot;0&quot; scrolling=&quot;auto&quot; allowtransparency=&quot;true&quot;>&lt;/iframe>
&lt;/script></form:htmls>
</form:Form>
