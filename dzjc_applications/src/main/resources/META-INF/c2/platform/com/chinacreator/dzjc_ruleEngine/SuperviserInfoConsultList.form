	}*/
	if($params.jcBeginDate!='' &amp;&amp; $params.jcBeginDate!=null){
		queryParams.push({field:&quot;superviseTime&quot;,op:&quot;ge&quot;,data:$params.jcBeginDate}) ;
	}
	if($params.jcEndDate!='' &amp;&amp; $params.jcEndDate!=null){
		if($params.jcBeginDate > $params.jcEndDate){
			Messenger.error(&quot;监管开始时间不能大于结束时间！&quot;);
			return;
		}
	queryParams.push({field:&quot;superviseTime&quot;,op:&quot;le&quot;,data:$params.jcEndDate}) ;
