  <form:scripts>

page_onload:function(){
	//已申诉的才显示意见的div
	if($model.entity.isComplain == &quot;Y&quot;){
		$(&quot;#agreeDiv&quot;).show();
		//查询意见信息
		$params.evalPointValueId = $model.entity.id;
		$http.get(c2.base('service')+&quot;dzjc_performance/v1/EvalRevieweAgree/getByEvalPointValueId/&quot;+$params.evalPointValueId).success(function(data){
			$model.agreeEntity = data;
		});
		
	}else{
		$(&quot;#agreeDiv&quot;).hide();
	}
},

btn_edit_click:function($event){
	Modal.instance.close();
}</form:scripts>
