  <form:scripts>buttonAdd_click:function(rowId,rowData,tableData){&#xD;
	Modal.open(&quot;f/dzjc_performance/createEvalFractionalAllocation&quot;,{},function(data){&#xD;
		$view.list.refresh();&#xD;
	});&#xD;
},&#xD;
buttonEdit_click:function(rowId,rowData,tableData){&#xD;
	if(rowId){&#xD;
		Modal.open(&quot;f/dzjc_performance/updateEvalFractionalAllocation&quot;,{id:rowId},function(data){&#xD;
			$view.list.refresh();&#xD;
		});&#xD;
	}else{&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}&#xD;
},&#xD;
buttonDelete_click:function(rowId,rowData,tableData){&#xD;
	var ids = $view.list.getSelectRowIds() ;&#xD;
	if(ids.length==0){&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}else{&#xD;
		Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},&#xD;
			function(data){&#xD;
				if(ids.length==1){&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_performance/v1/EvalFractionalAllocation/&quot;+ids[0]).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				}else{&#xD;
					 var queryStr=&quot;&quot;;&#xD;
					 for (var i =0; i&lt; ids.length;i++){&#xD;
						if(i==0){&#xD;
						   queryStr+=&quot;id=&quot;+ids[i] ;&#xD;
						}else{&#xD;
						   queryStr+=&quot;&amp;id=&quot;+ids[i];&#xD;
						}&#xD;
					  }&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_performance/v1/EvalFractionalAllocation?&quot;+queryStr).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				 }&#xD;
		});&#xD;
	}&#xD;
},

list_ondblClickRow:function(rowid,iRow,iCol,e){&#xD;
	var selectedRow = $view.list.getRowData(rowid);&#xD;
	console.log(selectedRow.allocationReason);
	Modal.instance.close(selectedRow);
},

button_click:function($event){&#xD;
	var selectedRow='';&#xD;
	var bigselectRow='';
	var ids = $view.list.getSelectRowIds();&#xD;
	if(ids.length&lt;=0){&#xD;
		Messenger.error('未选中扣分原因表格项!');&#xD;
		return;&#xD;
	}&#xD;
	if(ids.length>1){&#xD;
			var  Row = $view.list.getSelectDatas();&#xD;
			Modal.instance.close(Row);&#xD;
			&#xD;
	}else{&#xD;
			 selectedRow = $view.list.getRowData(ids);&#xD;
			Modal.instance.close(selectedRow);&#xD;
	}&#xD;
},

button1_click:function($event){
	Modal.instance.close();
},

list_loadComplete:function(data){
		for(var i=0; i&lt;data.contents.length; i++){&#xD;
		var  height = $('.model').eq(i).height();&#xD;
		console.log(height);&#xD;
		$('.model').eq(i).css({&quot;color&quot;:&quot;black&quot;,&quot;line-height&quot;:height+&quot;px&quot;});&#xD;
		$('.model1').eq(i).css({&quot;color&quot;:&quot;black&quot;,&quot;line-height&quot;:height+&quot;px&quot;});&#xD;
		$('.model2').eq(i).css({&quot;color&quot;:&quot;black&quot;,&quot;line-height&quot;:height+&quot;px&quot;});&#xD;
		$('.model3').eq(i).css({&quot;color&quot;:&quot;black&quot;,&quot;line-height&quot;:height+&quot;px&quot;});&#xD;
		$('.model4').eq(i).css({&quot;color&quot;:&quot;black&quot;,&quot;line-height&quot;:height+&quot;px&quot;});&#xD;
	}
}</form:scripts>
