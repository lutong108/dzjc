  <form:scripts>

button1_click:function($event){
	Modal.instance.close();
},

button_click:function($event){
	var superviseInfoId = $model.entity.superviseInfoId;
	//获取文本框的内容
	 var revocationReason = document.getElementById(&quot;reasonContent&quot;).value;
	 //&quot;撤销原因&quot;不能为空
	 if(revocationReason != null &amp;&amp; revocationReason != ''){
		 $http.post(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/SuperviseInfo/cxState&quot;,{superviseInfoId:superviseInfoId,revocationReason:revocationReason}).success(function(data){ 
	      if(data==&quot;1&quot;){
	    	  Modal.instance.close();
	    	  Messenger.post({type:'success',message:'撤销成功'});
	      }else{
	    	  Messenger.post({type:'success',message:'撤销失败'});
	      }
	    });
	 }else{
		 Messenger.error(&quot;请填写撤销原因！&quot;);
	 }
	
},

page_afterDataSourceLoad:function(){
	
	
	$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/SuperviseInfo/pdRoles&quot;).success(function(data){ 
      if(data==&quot;1&quot;){
    	  var state=$model.entity.state;
    	  var superviseResultNo=$model.entity.superviseResultNo;
    	  
    	  //2,监管结果为‘预警’时不显示撤销按钮和撤销原因
    	  //superviseResultNo:监管结果1-预警 2-黄牌 3-红牌
		  if(superviseResultNo!=&quot;2&quot; &amp;&amp;  superviseResultNo!=&quot;3&quot;){
			  document.getElementById(&quot;divcx&quot;).style.display=&quot;none&quot;;
			  document.getElementById(&quot;revocationReason&quot;).style.display=&quot;none&quot;;
		  }else{
			  //state:预警状态(已发Y,待发D,撤销C,取消Q，督办B)
			  if(state!=&quot;已发&quot;){
				  //3,非&quot;已发&quot;状态，&quot;撤销&quot;按钮不显示
				  document.getElementById(&quot;divcx&quot;).style.display=&quot;none&quot;;
				  if(state == &quot;撤销&quot;){
					  //4,&quot;撤销&quot;状态时，&quot;撤销原因&quot;不可编辑
					  document.getElementById(&quot;reasonContent&quot;).disabled=true;
				  }else{
					  //5,其他状态时，&quot;撤销原因&quot;不显示
					  document.getElementById(&quot;revocationReason&quot;).style.display=&quot;none&quot;;
				  }
			  }
		  }	 
      }else{
    	  document.getElementById(&quot;divcx&quot;).style.display=&quot;none&quot;;
    	  //1,&quot;撤销原因&quot;不显示
    	  document.getElementById(&quot;revocationReason&quot;).style.display=&quot;none&quot;;
      }
    });
	
	
}</form:scripts>
