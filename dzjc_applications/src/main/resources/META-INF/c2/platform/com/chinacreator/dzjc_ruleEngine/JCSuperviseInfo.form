  <form:datasources>
    <form:datasource xsi:type="form:EntityDataSource" id="entity" name="单实体数据源" base="service" sn="dzjc_ruleEngine/v1/SuperviseInfo" entity="entity:com.chinacreator.dzjc_ruleEngine.SuperviserInfo">
      <form:entityObj pk="supervise_info_id" restResource="true"/>
    </form:datasource>
