			var url = $('#gzt').attr(&quot;src&quot;);&#xD;
			if(url=='images/biaoti_zhangkai.png'){&#xD;
			$(&quot;#gzt&quot;)[0].src = 'images/biaoti_shouqi.png';&#xD;
			}else{&#xD;
				$(&quot;#gzt&quot;)[0].src = 'images/biaoti_zhangkai.png';&#xD;
