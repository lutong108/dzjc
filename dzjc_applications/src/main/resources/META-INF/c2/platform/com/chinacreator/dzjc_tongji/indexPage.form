			if(url=='images/biaoti_zhangkai.png'){&#xD;
			$(&quot;#success&quot;)[0].src = 'images/biaoti_shouqi.png';&#xD;
			}else{&#xD;
				$(&quot;#success&quot;)[0].src = 'images/biaoti_zhangkai.png';&#xD;
