projectState_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){&#xD;
	var str = &quot;&quot;;&#xD;
	if(!angular.isUndefined(cellvalue)){&#xD;
		if(cellvalue == &quot;0&quot;){&#xD;
			str = &quot;暂存&quot;;&#xD;
		}else if(cellvalue == &quot;1&quot;){&#xD;
			str = &quot;受理&quot;;&#xD;
		}else if(cellvalue == &quot;2&quot;){&#xD;
			str = &quot;不予受理&quot;;&#xD;
		}else if(cellvalue == &quot;3&quot;){&#xD;
			str = &quot;补正补齐&quot;;&#xD;
		}else if(cellvalue == &quot;4&quot;){&#xD;
			str = &quot;业务办结&quot;;&#xD;
		}else if(cellvalue == &quot;5&quot;){&#xD;
			str = &quot;统一办结&quot;;&#xD;
		}else if(cellvalue == &quot;6&quot;){&#xD;
			str = &quot;办结&quot;;&#xD;
		}else if(cellvalue == &quot;7&quot;){&#xD;
			str = &quot;作废办结&quot;;&#xD;
		}else if(cellvalue == &quot;9&quot;){&#xD;
			str = &quot;网上提交&quot;;&#xD;
		}else if(cellvalue == &quot;10&quot;){&#xD;
			str = &quot;预审通过&quot;;&#xD;
		}else if(cellvalue == &quot;11&quot;){&#xD;
			str = &quot;预审不通过&quot;;&#xD;
		}&#xD;
	}&#xD;
	return str;&#xD;
},&#xD;
