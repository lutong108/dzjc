list1_ondblClickRow:function(rowid,iRow,iCol,e){
  //获取整行数据&#xD;
  var rowData = $view.list1.getRowData(rowid);&#xD;
  var typeId = rowData.jcTypeId;//类型编号&#xD;
  var resultId = rowData.jcResultId;//状态编号&#xD;
  var superviseInfoId=rowData.superviseInfoId;&#xD;
      &#xD;
  &#xD;
  //投诉业务处理&#xD;
  if(typeId == &quot;1000&quot;){&#xD;
    if(resultId == &quot;0&quot;){//预受理&#xD;
      Modal.open(&quot;f/dzjc_complain/taJcComplainPreAcceptInfo&quot;,{'complainId':rowid},function(data){&#xD;
        $view.list.refresh({});&#xD;
      });&#xD;
    &#xD;
    }else if(resultId == &quot;1&quot;){//待受理&#xD;
      Modal.open(&quot;f/dzjc_complain/taJcComplainAcceptInfo&quot;,{'complainId':rowid},function(data){&#xD;
        $view.list.refresh({});&#xD;
      });&#xD;
    }&#xD;
    else if(resultId == &quot;4&quot;){//待接收&#xD;
      Modal.open(&quot;f/dzjc_complain/taJcComplainJieshouInfo&quot;,{'complainId':rowid},function(data){&#xD;
        $view.list.refresh({});&#xD;
      });&#xD;
    }&#xD;
    else if(resultId == &quot;5&quot;){//待回复&#xD;
      Modal.open(&quot;f/dzjc_complain/taJcComplainChuliInfo&quot;,{'complainId':rowid},function(data){&#xD;
        $view.list.refresh({});&#xD;
      });&#xD;
    }&#xD;
    else if(resultId == &quot;6&quot; || resultId == &quot;7&quot; || resultId == &quot;8&quot;){//待审核&#xD;
      Modal.open(&quot;f/dzjc_complain/taJcComplainShenheInfo&quot;,{'complainId':rowid},function(data){&#xD;
        $view.list.refresh({});&#xD;
      });&#xD;
    }&#xD;
  &#xD;
  &#xD;
  //监管申诉处理&#xD;
  }else if(typeId == &quot;1001&quot;){&#xD;
    $http.post(c2.base(&quot;service&quot;)+&quot;dzjc_dealtMatter/v1/VJcDaiban/getSuperviserInfo&quot;,{dbId:rowid}).success(function(data){ &#xD;
        if(data==&quot;1011&quot; || data==&quot;1014&quot; || data==&quot;1016&quot; || data==&quot;1017&quot; || data==&quot;1018&quot;){&#xD;
          Modal.open(&quot;f/dzjc/TaJcWarningAppeals&quot;,{id:rowid,resultId:resultId},function(data){&#xD;
        $view.list1.refresh({});&#xD;
          });&#xD;
        }else if(data==&quot;1012&quot;){&#xD;
          Modal.open(&quot;f/dzjc/TaJcWarningAppealCom&quot;,{id:rowid,resultId:resultId},function(data){&#xD;
        $view.list1.refresh({});&#xD;
          });&#xD;
        }else if(data==&quot;1013&quot; || data==&quot;1015&quot;){&#xD;
           Modal.open(&quot;f/dzjc/TaJcWarningAppealCon&quot;,{id:rowid,resultId:resultId},function(data){&#xD;
        $view.list1.refresh({});&#xD;
          });&#xD;
        }&#xD;
      });&#xD;
  //人工监管发牌申诉处理&#xD;
  }else if(typeId == &quot;1002&quot;){&#xD;
    Modal.open(&quot;f/dzjc_dealtMatter/humanappealInforeply&quot;,{id:rowid,superviseInfoId:superviseInfoId},function(data){&#xD;
        $view.list1.refresh({});&#xD;
          });&#xD;
  }else{&#xD;
    Modal.open(&quot;f/dzjc/SuperviseInfo&quot;,{id:rowData.superviseInfoId},function(data){&#xD;
      $view.list1.refresh({});&#xD;
    });&#xD;
  }
},&#xD;
