//初始化控件&#xD;
initDateTimeRangePicker:function(){&#xD;
	$model.list.beginDate = '';&#xD;
	$model.list.endDate = '';&#xD;
	var date_ = new Date();  &#xD;
	date_.setDate(1); &#xD;
    var timeBegin = date_.getTime();&#xD;
	$model.list.jcBeginDate = timeBegin;&#xD;
    var currentMonth=date_.getMonth();&#xD;
    var nextMonth=++currentMonth;&#xD;
 	var nextMonthFirstDay=new Date(date_.getFullYear(),nextMonth,1);&#xD;
 	var t=new Date(); &#xD;
 	t.setTime(nextMonthFirstDay-1000*60*60*24);&#xD;
 	var timeEnd = t.getTime(); &#xD;
 	$model.list.jcEndDate = timeEnd; &#xD;
 	&#xD;
},&#xD;
//毫秒值转yyyy-MM-dd格式日期&#xD;
setDefaultDate:function(millisecond) {&#xD;
	var odate = new Date(parseInt(millisecond));&#xD;
	var month =  odate.getMonth() + 1;&#xD;
	var day =  odate.getDate();&#xD;
	month = (month.toString().length == 1) ? (&quot;0&quot; + month) : month;&#xD;
	day = (day.toString().length == 1) ? (&quot;0&quot; + day) : day;&#xD;
&#xD;
	var result =  odate.getFullYear() + '-' + month + '-' + day;&#xD;
&#xD;
	return result;&#xD;
},
