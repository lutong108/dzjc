//办理状态
bjState_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	//'1','在办','9','在办','10','在办','11','已办结','2','已办结','4','已办结','5','已办结','6','已办结','7','已办结','8','已办结'&#xD;
