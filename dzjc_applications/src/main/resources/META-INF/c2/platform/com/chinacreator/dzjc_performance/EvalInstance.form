  <form:scripts>

buttonAdd_click:function(rowId,rowData,tableData){
	Modal.open(&quot;f/dzjc_performance/createEvalInstance&quot;,{},function(data){&#xD;
		$view.list.refresh({&quot;areaCode&quot;:$params.areaCode});&#xD;
	});
},

buttonEdit_click:function(rowId,rowData,tableData){
	var ids = $view.list.getSelectRowIds();
	if(ids.length > 1){
		Messenger.error(&quot;编辑只能选中一行数据!&quot;);
		return;
	}
	if(rowId){&#xD;
		Modal.open(&quot;f/dzjc_performance/updateEvalInstance2&quot;,{id:rowId,evalObjectType:rowData.evalObjectType},function(data){&#xD;
			$view.list.refresh({&quot;areaCode&quot;:$params.areaCode});&#xD;
		});&#xD;
	}else{&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}
},

buttonDelete_click:function(rowId,rowData,tableData){
	var ids = $view.list.getSelectRowIds();&#xD;
	if(ids.length==0){&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}else{&#xD;
		Modal.openDelConfirm({message:&quot;删除实例的同时会删除和该实例相关的指标成绩和考核对象信息，确定是否删除！&quot;},&#xD;
			function(data){&#xD;
				if(ids.length==1){&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_performance/v1/evalInstance/&quot;+ids[0]).success(function(data){&#xD;
						if(data != null &amp;&amp; data != '' &amp;&amp; data == 'success'){
							$view.list.refresh({&quot;areaCode&quot;:$params.areaCode});
							Messenger.success(&quot;删除成功！&quot;);
						}else{
							$view.list.refresh({&quot;areaCode&quot;:$params.areaCode});
							Messenger.error(&quot;删除失败！&quot;);
						}&#xD;
					});&#xD;
				}else{&#xD;
					 var queryStr=&quot;&quot;;&#xD;
					 for (var i =0; i&lt; ids.length;i++){&#xD;
						if(i==0){&#xD;
						   queryStr+=&quot;id=&quot;+ids[i] ;&#xD;
						}else{&#xD;
						   queryStr+=&quot;&amp;id=&quot;+ids[i];&#xD;
						}&#xD;
					  }&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_performance/v1/evalInstance?&quot;+queryStr).success(function(data){&#xD;
						if(data != null &amp;&amp; data != '' &amp;&amp; data == 'success'){
							$view.list.refresh({&quot;areaCode&quot;:$params.areaCode});
							Messenger.success(&quot;删除成功！&quot;);
						}else{
							$view.list.refresh({&quot;areaCode&quot;:$params.areaCode});
							Messenger.error(&quot;删除失败！&quot;);
						}&#xD;
					});&#xD;
				 }&#xD;
		});&#xD;
	}
},

//重置按钮
button1_click:function($event){
	$model.list.orgId = null;
	$view.mainForm.clean();
	$view.list.refresh({&quot;areaCode&quot;:$params.areaCode});
},

//查询按钮
button_click:function($event){
	var queryParams = [];
	if($model.list.tableName!=&quot;&quot; &amp;&amp; $model.list.tableName != null){
		queryParams.push({field:'tableName',op:'eq',data:$model.list.tableName});
	}
	if($model.list.isValid != '' &amp;&amp; $model.list.isValid != null){
		queryParams.push({field:'isValid',op:'eq',data:$model.list.isValid});
	}
	if($model.list.orgId != '' &amp;&amp; $model.list.orgId != null){
		queryParams.push({field:'orgId',op:'eq',data:$model.list.orgId});
	}
	if($model.list.evalObjectType != '' &amp;&amp; $model.list.evalObjectType != null){
		queryParams.push({field:'evalObjectType',op:'eq',data:$model.list.evalObjectType});
	}
	if(queryParams.length != 0){
		var filters = {groupOp:'AND',rules:queryParams};
		$view.list.refresh({filters:filters,&quot;areaCode&quot;:$params.areaCode});
	}else{
		$view.list.refresh({&quot;areaCode&quot;:$params.areaCode});
	}
},

