btn_update_click:function($event){
	
	if($model.entity.compReasons == null || $model.entity.compReasons == ''){
		
		Messenger.error(&quot;申诉原因不能为空&quot;);
		$(&quot;#compReasons&quot;).focus();
		return;
	}

	/*$model.entity.$save(function(){
		Modal.instance.close();
		Messenger.post({type:'success',message:'申诉成功'});
	});*/
	$http.post(c2.base('service')+&quot;dzjc_performance/v1/evalPointValue/updateRevieweStates&quot;,$model.entity).success(function(data){
		Modal.instance.close();
		Messenger.post({type:'success',message:'申诉成功'});
	});
},
btn_edit_click:function($event){
	Modal.instance.close();
},


