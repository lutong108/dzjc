  <form:scripts>//点击查询
query_button_click:function($event){
	var queryParams = new Array();

	//标题信息
	if($model.list.title!= '' &amp;&amp; $model.list.title!= null){
		queryParams.push({field:&quot;title&quot;,op:&quot;cn&quot;,data:$model.list.title}) ;
	}
	//标题信息
	if($model.list.queryCode!= '' &amp;&amp; $model.list.queryCode!= null){
		queryParams.push({field:&quot;queryCode&quot;,op:&quot;eq&quot;,data:$model.list.queryCode}) ;
	}
	
	//监管类型
	if($model.list.superviseTypeName!= '' &amp;&amp; $model.list.superviseTypeName!= null){
		queryParams.push({field:&quot;superviseTypeNo&quot;,op:&quot;eq&quot;,data:$model.list.superviseTypeName}) ;
	}
	
	//监管结果
	if($model.list.superviseResultName!= '' &amp;&amp; $model.list.superviseResultName!= null){
		queryParams.push({field:&quot;superviseResultNo&quot;,op:&quot;eq&quot;,data:$model.list.superviseResultName}) ;
	}
	
	//申诉状态
	if($model.list.suState!= '' &amp;&amp; $model.list.suState!= null){
		queryParams.push({field:&quot;suState&quot;,op:&quot;eq&quot;,data:$model.list.suState}) ;
	}
	
	if(beginDate.value!= '' &amp;&amp; beginDate.value!= null){
		queryParams.push({field:&quot;superviseTime&quot;,op:&quot;ge&quot;,data:beginDate.value}) ;
	}
	if(endDate.value!= '' &amp;&amp; endDate.value!= null){
	
		if(beginDate.value > endDate.value){
			Messenger.error(&quot;开始时间不能大于结束时间！&quot;);
			return;
		}
	
		queryParams.push({field:&quot;superviseTime&quot;,op:&quot;le&quot;,data:endDate.value}) ;
	}
	
	if(queryParams.length != 0 ){
		var filters={groupOp:&quot;AND&quot;,rules:queryParams}
		$view.list.refresh({filters:filters});
	}else{
		$view.list.refresh({filters:filters});
	}
},

//点击重置
clean_button_click:function($event){
	$view.mainForm.clean();
	$view.list.refresh({});
},


//双击行，弹出信息
