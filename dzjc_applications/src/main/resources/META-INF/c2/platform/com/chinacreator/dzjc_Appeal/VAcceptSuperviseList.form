		Modal.open(&quot;f/dzjc/SuperviseInfoBanJian&quot;,{id:rowid,&quot;superviseInfoId&quot;:rowData.businessId,&quot;suState&quot;:rowData.suState},function(data){
			functions.query_button_click();
		});
	
	//投诉监管信息
	}else if(rowData.superviseTypeNo == &quot;1012&quot;){
		Modal.open(&quot;f/dzjc/SuperviseInfoTouSu&quot;,{id:rowid,&quot;superviseInfoId&quot;:rowData.businessId,&quot;suState&quot;:rowData.suState},function(data){
