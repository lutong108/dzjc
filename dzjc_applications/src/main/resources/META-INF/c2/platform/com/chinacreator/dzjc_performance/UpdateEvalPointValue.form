  <form:scripts>//保存点击事件
btn_update_click:function($event){
	//数据的非空校验
	if($model.entity.subtraction!=0){
		if($model.entity.subtraction == null || $model.entity.subtraction == ''  ){
			Messenger.error(&quot;本次减分不能为空！&quot;);
			$(&quot;#subtraction&quot;).focus();
			return;
		}
	}
	if($model.entity.deductReasons == null || $model.entity.deductReasons == '' ){
		Messenger.error(&quot;扣分原因不能为空！&quot;);
		$(&quot;#deductReasons&quot;).focus();
		return;
	}
	var re = /^[0-9]+.?[0-9]*$/; //判断字符串是否为数字 //判断正整数 /^[1-9]+[0-9]*]*$/ 
　　   var nubmer = $model.entity.subtraction;
	if(nubmer!=undefined){
		   　if (!re.test(nubmer)) {
		　　 		Messenger.error('本次减分不能为负数!');
		　　　　return false;
		　　 }
	 }
	  var deductReasons= $model.entity.deductReasons;
	 	 deductReasons = deductReasons.split(&quot;&quot;);
	 	 if(deductReasons.length>2000){
	 		Messenger.error(&quot;扣分原因超过2000字!&quot;);
	 		return;
	 	 }
	 Modal.openConfirm({message:&quot;该考核对象评分保存后将无法修改，是否确认保存&quot;},function(data){
			   $model.entity.$save(function(){
				Modal.instance.close();
				Messenger.post({type:'success',message:'评分成功'});
			  });
	 });
},
btn_edit_click:function($event){
	Modal.instance.close();
},
//数据源加载前事件
page_beforeDataSourceLoad:function($params){
	$http.get(c2.base(&quot;service&quot;)+&quot;dzjc_performance/v1/dicEvalPoint/&quot;+$params.pointId).success(function(data){
		$model.entity.pointScore=data.maxValue;
	});
},
//数据上传事件
button_click:function($event){

	$view.fileInput.submit({evalObjectName:$model.entity.evalObjectName,id:$params.id});
},
//上传成功事件
fileInput_onSuccess:function(data){
	$view.fileInput.reset();
	$view.list.refresh({&quot;id&quot;:$params.id,&quot;isShow&quot;:&quot;Y&quot;});
	Messenger.post({type:'success',message:'上传成功'});
},
//上传失败事件
fileInput_onError:function(data){
	$view.fileInput.reset();
	$view.list.refresh({&quot;id&quot;:$params.id});
	Messenger.post({type:'error',message:'上传失败'});
},
//自定义格式
colJqgrid_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	var htmls = '&lt;input class=&quot;button&quot; title=&quot;预览&quot; ng-click=&quot;functions.attach_download(rowObject)&quot; type=&quot;button&quot;  value=&quot;预览&quot;/>';
	return htmls;
},
//预览
attach_download:function(rowObject){
	Modal.open(&quot;f/dzjc_performance/OpenView&quot;,{path:rowObject.pathUrl},function(data){
	});
},
//扣分原因处理事件
deductReasons_click:function($event){
	var value=$model.list.isShow;
	if(value=='Y'){
		Modal.open(&quot;f/dzjc_performance/EvalFractionalAllocationOpen&quot;,{allocationType:1,allocationExamination:$model.entity.pointName},function(data){
		if(data.length>1){
		var appendString='';
			for(var i=0; i&lt;data.length; i++){
				appendString+=data[i].allocationReason;
				
			}
			$model.entity.deductReasons=appendString;
		}else{
			$model.entity.deductReasons=data.allocationReason;
		}	
	});
	}
},
//扣分原因模式处理事件
radioGroup_clickEvent:function(value){
	$model.entity.deductReasons='';
},
//页面加载完成事件
page_onload:function(){
	$model.entity.remark='';
	$model.entity.deductReasons='';
	$model.entity.subtraction='';
}</form:scripts>
