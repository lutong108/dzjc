  <form:scripts>
//监管信息双击行
list1_ondblClickRow:function(rowid,iRow,iCol,e){
	if(rowid){
		Modal.open(&quot;f/dzjc_ruleEngine/jcSuperviseInfo&quot;,{id:rowid},function(data){
			$view.listJc.refresh();
		});
	}else{ 
	
		Messenger.error(&quot;请选中表格项!&quot;);
	}	
},
//关闭按钮
button1_click:function($event){
	Modal.instance.close();
},

buttonATable_click:function(rowId,rowData,tableData){
	var bussinessId=$model.entity.instanceId;
	var orgId=$model.entity.orgId;
	var bussinessType='1011';
	Modal.open(&quot;f/dzjc_ruleEngine/rGFPSuperviseInfo&quot;,{bussinessId:bussinessId,bussinessType:bussinessType,orgId:orgId},function(data){
		$view.listJc.refresh();
	});
	
},
//数据源加载后
page_afterDataSourceLoad:function(){

	//查询老事项库的数据
	var approveId = $params.approveId;
	
	if(approveId != 'undefined' &amp;&amp; approveId != null &amp;&amp; approveId != ''){
		$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_tongji/v1/getSystemAreaCode&quot;).success(function(result){
			if(result.success){	
				var SYSTEM_AREACODE_VALUE = result.SYSTEM_AREACODE_VALUE;
				if(SYSTEM_AREACODE_VALUE=='4300000000'){
					 $.ajax({
				         url:c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/superviseGuideOld/&quot;+approveId,
				         type:&quot;GET&quot;,
				         dataType:&quot;JSON&quot;,
				         success:function(data,status){
					         if(data != undefined){
					        	 var approveLimit = data.approveLimit;
					        	 $params.timeLimit = approveLimit;
					        	 var commitmentLimit = data.commitmentLimit;
					        	 $params.promisesLimit = commitmentLimit;
					        	 var etlTime = $params.etlTime;
					         }
				         } ,
				         error:function(request,status,errorMsg){
				        	 Messenger.error(&quot;查询老事项库数据错误！&quot;);
				         }
					});
				 }else{
					 /*只有省级监察系统显示老的承若时限和法定时限*/
					$('#timeLimit_oldWrapper').hide();
					$('#promisesLimit_oldWrapper').hide();
				 }
			}else{
				Messenger.error({type:'error',message:'程序异常'});
			}
		});
	}

	$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/SuperviseInfo/pdRoles&quot;).success(function(data){ 
      if(data==&quot;1&quot;){
    	  $http.post(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/SuperviseInfo/areaCode&quot;).success(function(data){ 
    		  if(data==&quot;4300000000&quot;){
    			  $(&quot;#buttonATable&quot;).hide();
    		  }else{
    			  $(&quot;#buttonATable&quot;).show();
    		  }
    	  });
      }else{
    	  document.getElementById(&quot;buttonATable&quot;).style.display=&quot;none&quot;;
      }
    });
    console.log($params.superviseTypeNo);
    if($params.superviseTypeNo==&quot;1017&quot;){
    	$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/allInfoWebUseTime&quot;,
    	JSON.stringify({&quot;businessId&quot;:$params.businessId,&quot;superviseResultNo&quot;:$params.superviseResultNo,
    	&quot;superviseTypeNo&quot;:$params.superviseTypeNo}))
    	.success(function(data){
			$model.remainTimeLimit=(data.timeLimit-0)-(data.outDays-0);
			$model.remainPromiseTime=(data.promisesLimit-0)-(data.outDays-0);
			$model.entity.timeLimit=data.timeLimit
			console.log(data.timeLimit);
			$model.entity.promisesLimit=data.promisesLimit;
			
		});
    }else{
    	if($params.superviseResultNo == &quot;3&quot;&amp;&amp;(IsSpace($model.entity.endTime)|| $model.entity.endTime>$params.superviseTime)){
			var obj=new Object();
			obj.endTime=$model.entity.endTime;
			obj.acceptTime=$model.entity.acceptTime;
			obj.instanceId=$model.entity.instanceId;
			$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/getUseTime&quot;,obj).success(function(data){
				if(!$model.entity.timeLimit){
					$model.remainTimeLimit=&quot;&quot;;
				}
				else {
					var processsLimit = Math.ceil($model.entity.timeLimit);
					$model.remainTimeLimit=(processsLimit-0)-(data-0);
				}
				if(!$model.entity.promisesLimit){
					$model.remainPromiseTime=&quot;&quot;;
				}
				else {
					var promiseLimit = Math.ceil($model.entity.promisesLimit);
					$model.remainPromiseTime=(promiseLimit-0)-(data-0);
				}
			});
		}else{
			if(!$model.entity.timeLimit){
				$model.remainTimeLimit=&quot;&quot;;
			}else{
				var processsLimit = Math.ceil($model.entity.timeLimit);
				$model.remainTimeLimit=(processsLimit-0)-($model.entity.timeUse-0);
			}
			if(!$model.entity.promisesLimit){
				$model.remainPromiseTime=&quot;&quot;;
			}else{
				var promiseLimit = Math.ceil($model.entity.promisesLimit);
				$model.remainPromiseTime=(promiseLimit-0)-($model.entity.timeUse-0);
			}
		}
    }
	
},

opinionTime1_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(cellvalue!=null &amp;&amp; cellvalue!=undefined &amp;&amp; cellvalue!=&quot;&quot;){
		return new Date(cellvalue).Format(&quot;yyyy-MM-dd hh:mm:ss&quot;);
	}else{
		return &quot;&quot;;
	}
},

superviseTime1_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	//空返回false
	if(isEmptyValue(cellvalue)){
		return new Date(cellvalue).Format(&quot;yyyy-MM-dd hh:mm:ss&quot;);
	}else{
		return &quot;&quot;;
	}
},
//监管结果自定义格式
superviseResultName2_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(rowObject.superviseResultName==&quot;预警&quot;){
	return &quot;&lt;img style='margin-left: 7px; width:15px;'src='dzjc/images/table_icon02.gif' alt='预警'  /> &quot;;
	}else if(rowObject.superviseResultName==&quot;黄牌&quot;){
	return &quot;&lt;img style='margin-left: 3px; width:15px;' src='dzjc/images/table_icon03.gif' alt='黄牌'/>&quot;;
	}else{
	return &quot;&lt;img style='margin-left: 3px; width:15px;' src='dzjc/images/table_icon04.gif' alt='红牌'/>&quot;;
	}
},
//页面加载完成
page_onload:function(){
	if($params.superviseResultNo == &quot;1&quot;){
		$(&quot;#jcfpimg&quot;)[0].src = 'dzjc/images/warn.gif';
	}else if($params.superviseResultNo == &quot;2&quot;){
		$(&quot;#jcfpimg&quot;)[0].src='dzjc/images/yellower.gif';
	}else if($params.superviseResultNo == &quot;3&quot;){
		$(&quot;#jcfpimg&quot;)[0].src='dzjc/images/red.gif';
	}else {
		$(&quot;#jcfpimg&quot;)[0].src='dzjc/images/normally.gif';
	}
	if(!IsSpace($model.entity.etlTime)){ 
		$(&quot;#etlTimeDiv&quot;).show();
	}
},
//监管环节双击行
resultOpinionList1_ondblClickRow:function(rowid,iRow,iCol,e){
	if(rowid){
		var tacheName=$view.resultOpinionList1.getSingleSelectData().tacheName;
		var instanceId=$view.resultOpinionList1.getSingleSelectData().instanceId;
		var opinionTime=$view.resultOpinionList1.getSingleSelectData().opinionTime;
		var opinionId=$view.resultOpinionList1.getSingleSelectData().opinionId;
		if(tacheName==&quot;受理&quot;){
			//Modal.open()dzjc_ruleEngine/acceptTacheInfo
			Modal.open(&quot;f/dzjc_ruleEngine/acceptTacheInfo&quot;,{instanceId:instanceId,acceptTime:opinionTime},function(data){
			});
		}else if(tacheName.indexOf(&quot;办结&quot;) >= 0){ 
			Modal.open(&quot;f/dzjc_ruleEngine/endTacheInfo&quot;,{instanceId:instanceId,endTime:opinionTime,opinionId:opinionId},function(data){
			});
		}else if(tacheName.indexOf(&quot;特别程序申请&quot;)>=0){
			Modal.open(&quot;f/dzjc_ruleEngine/suspendTacheInfo&quot;,{opinionId:opinionId},function(data){
			});
		}else if(tacheName.indexOf(&quot;特别程序结束&quot;)>=0){
			Modal.open(&quot;f/dzjc_ruleEngine/resumeTacheInfo&quot;,{opinionId:opinionId},function(data){
			});
		}else{		//过程
			Modal.open(&quot;f/dzjc_ruleEngine/processTacheInfo&quot;,{opinionId:opinionId,tacheName:tacheName},function(data){
			});
		}
	}
}
