    <form:datasource xsi:type="form:EntityDataSource" id="entity" name="实体数据源" base="service" sn="dzjc_ruleEngine/v1/Superviseinfo_huanjie" data="$params.superviseInfoId" entity="entity:com.chinacreator.dzjc_ruleEngine.Superviseinfo_huanjie">
      <form:entityObj pk="instance_id" restResource="true"/>
    </form:datasource>
    <form:datasource xsi:type="form:PagingDataSource" id="list1" name="列表型数据源" base="service" sn="dzjc_ruleEngine/v1/SuperviseInfo" elementType="entity:com.chinacreator.dzjc_ruleEngine.SuperviserInfo" pageSize="10">
      <form:entityObj pk="supervise_info_id" restResource="true"/>
    </form:datasource>
    <form:datasource xsi:type="form:PagingDataSource" id="resultOpinionList" name="审批结果列表数据源" base="service" sn="dzjc_ruleEngine/v1/tacheView" elementType="entity:com.chinacreator.dzjc_ruleEngine.TacheView" pageSize="10">
      <form:entityObj pk="id" restResource="true"/>
    </form:datasource>
    <form:datasource xsi:type="form:PagingDataSource" id="list2" name="列表型数据源" base="service" sn="dzjc_ruleEngine/v1/Superviseinfo_Message" elementType="entity:com.chinacreator.dzjc_ruleEngine.Superviseinfo_Message" pageSize="10">
      <form:entityObj pk="supervise_info_id" restResource="true"/>
    </form:datasource>
  </form:datasources>
  <form:scripts>
//监管信息双击事件
list1_ondblClickRow:function(rowid,iRow,iCol,e){
	if(rowid){
		Modal.open(&quot;f/dzjc_ruleEngine/updateSuperviser&quot;,{id:rowid},function(data){
			$view.list.refresh();
		});
	}else{
		Messenger.error(&quot;请选中表格项!&quot;);
	}	
},
//关闭按钮
button1_click:function($event){
	Modal.instance.close();
},
//数据源加载后
page_afterDataSourceLoad:function(){
	var timeUse =$model.entity.timeUse;
	if(timeUse==null || timeUse==&quot;&quot; || timeUse==undefined){
		timeUse=0;
	}
	if($params.superviseResultNo == &quot;3&quot;&amp;&amp;(IsSpace($model.entity.endTime)|| $model.entity.endTime>$params.superviseTime)){
		var obj=new Object();
		obj.endTime=$model.entity.endTime;
		obj.acceptTime=$model.entity.acceptTime;
		obj.instanceId=$model.entity.instanceId;
		$http.post(c2.base(&quot;service&quot;)+&quot;dzjc_ruleEngine/v1/getUseTime&quot;,obj).success(function(data){
			$model.remainTimeLimit=($model.entity.timeLimit-0)-(data-0);
			$model.remainPromiseTime=($model.entity.promisesLimit-0)-(data-0);
		});
	}else{
		if(!$model.entity.timeLimit){
			$model.remainTimeLimit=&quot;&quot;;
		}else{
			$model.remainTimeLimit=($model.entity.timeLimit-0)-timeUse;
		}
		if(!$model.entity.promisesLimit){
			$model.remainPromiseTime=&quot;&quot;;
		}else{
			$model.remainPromiseTime=($model.entity.promisesLimit-0)-timeUse;
		}
	}
	
},
//页面加载完成
page_onload:function(){
	debugger;
	if($model.entity.superviseResultNo == &quot;1&quot;){
		$(&quot;#jcfpimg&quot;)[0].src = 'dzjc/images/warn.gif';
	}else if($model.entity.superviseResultNo == &quot;2&quot;){
		$(&quot;#jcfpimg&quot;)[0].src='dzjc/images/yellower.gif';
	}else if($model.entity.superviseResultNo == &quot;3&quot;){
		$(&quot;#jcfpimg&quot;)[0].src='dzjc/images/red.gif';
	}
	/*else {
		$(&quot;#jcfpimg&quot;)[0].src='dzjc/images/normally.gif';
	}*/
	//网上办事监管
	if($params.superviseTypeCode==&quot;1017&quot;){
		$(&quot;#submitTimediv&quot;).show();
	}
	var str=	$(&quot;#resultOpinionList&quot;).find(&quot;tbody&quot;).find(&quot;tr&quot;);
 
	 for (var i=0; i&lt;str.length; i++){
		 var linkflag= $(str[i]).children(&quot;td:first&quot;).text();
		 console.log(linkflag);
		 if(linkflag==2){
		 $(str[i]).css(&quot;background-color&quot;,&quot;yellow&quot;);
		 }
	 }
	
},

//监管信息监管结果自定义
superviseResultName_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(rowObject.superviseResultName==&quot;预警&quot;){
	return &quot;&lt;img style='margin-left: 7px; width:15px;'src='dzjc/images/table_icon02.gif' alt='预警'  /> &quot;;
	}else if(rowObject.superviseResultName==&quot;黄牌&quot;){
	return &quot;&lt;img style='margin-left: 3px; width:15px;' src='dzjc/images/table_icon03.gif' alt='黄牌'/>&quot;;
	}else{
	return &quot;&lt;img style='margin-left: 3px; width:15px;' src='dzjc/images/table_icon04.gif' alt='红牌'/>&quot;;
	}
},
//办理时间自定义
opinionTime_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(cellvalue!=null &amp;&amp; cellvalue!=undefined &amp;&amp; cellvalue!=&quot;&quot;){
		return new Date(cellvalue).Format(&quot;yyyy-MM-dd hh:mm:ss&quot;);
	}else{
		return &quot;&quot;;
	}
},

superviseTime_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(isEmptyValue(cellvalue)){
		return new Date(cellvalue).Format(&quot;yyyy-MM-dd HH:mm:ss&quot;);
	}else{
		return &quot;&quot;;
	}
},
//监管环节信息双击行
resultOpinionList_ondblClickRow:function(rowid,iRow,iCol,e){
	if(rowid){
		var tacheName=$view.resultOpinionList.getSingleSelectData().tacheName;
		var instanceId=$view.resultOpinionList.getSingleSelectData().instanceId;
		var opinionTime=$view.resultOpinionList.getSingleSelectData().opinionTime;
		var opinionId=$view.resultOpinionList.getSingleSelectData().opinionId;
		if(tacheName==&quot;受理&quot;){
			//Modal.open()dzjc_ruleEngine/acceptTacheInfo
			Modal.open(&quot;f/dzjc_ruleEngine/acceptTacheInfo&quot;,{instanceId:instanceId,acceptTime:opinionTime},function(data){
			});
		}else if(tacheName.indexOf(&quot;办结&quot;) >= 0){ 
			Modal.open(&quot;f/dzjc_ruleEngine/endTacheInfo&quot;,{instanceId:instanceId,endTime:opinionTime,opinionId:opinionId},function(data){
			});
		}else if(tacheName.indexOf(&quot;特别程序申请&quot;)>=0){
			Modal.open(&quot;f/dzjc_ruleEngine/suspendTacheInfo&quot;,{opinionId:opinionId},function(data){
			});
		}else if(tacheName.indexOf(&quot;特别程序结束&quot;)>=0){
			Modal.open(&quot;f/dzjc_ruleEngine/resumeTacheInfo&quot;,{opinionId:opinionId},function(data){
			});
		}else{		//过程
			Modal.open(&quot;f/dzjc_ruleEngine/processTacheInfo&quot;,{opinionId:opinionId,tacheName:tacheName},function(data){
			});
		}
	}
},
//环节监管结果自定义
superviseResultNo_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(rowObject.superviseResultNo==&quot;1&quot;){
	return &quot;&lt;img style='margin-left: 7px; width:15px;'src='dzjc/images/table_icon02.gif' title='预警' alt='预警'  /> &quot;;
	}else if(rowObject.superviseResultNo==&quot;2&quot;){
	return &quot;&lt;img style='margin-left: 3px; width:15px;' src='dzjc/images/table_icon03.gif' title='黄牌' alt='黄牌'/>&quot;;
	}else if (rowObject.superviseResultNo==&quot;3&quot;){
	return &quot;&lt;img style='margin-left: 3px; width:15px;' src='dzjc/images/table_icon04.gif' title='红牌' alt='红牌'/>&quot;;
	}
},
//处理状态自定义
state_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	//预警状态(已发Y,待发D,撤销C,取消Q，督办B)
	if(rowObject.state==&quot;Y&quot;){
		cellvalue='已发';
	}else if(rowObject.state==&quot;D&quot;){
		cellvalue='待发';
	}else if(rowObject.state==&quot;C&quot;){
		cellvalue='撤销';
	}else if(rowObject.state==&quot;Q&quot;){
		cellvalue='取消';
	}else if(rowObject.state==&quot;B&quot;){
		cellvalue='督办';
	}else{
	cellvalue='';
	}
	return cellvalue;
	
},
//监管类型自定义
superviseTypeNo_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	if(rowObject.superviseTypeNo==&quot;1019&quot;){
		cellvalue='环节时限监管';
	}else{
		cellvalue='';
	}
	return cellvalue;
},
//环节信息双击事件
list2_ondblClickRow:function(rowid,iRow,iCol,e){
	debugger;
	if(rowid){
		Modal.open(&quot;f/dzjc_ruleEngine/updateSuperviser_Message&quot;,{id:rowid},function(data){
			$view.list.refresh();
		});
	}else{
		Messenger.error(&quot;请选中表格项!&quot;);
	}
}</form:scripts>
