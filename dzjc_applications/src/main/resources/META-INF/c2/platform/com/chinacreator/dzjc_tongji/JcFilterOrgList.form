  <form:scripts>buttonAdd_click:function(rowId,rowData,tableData){&#xD;
	Modal.open(&quot;f/dzjc_tongji/createJcFilterOrg&quot;,{},function(data){&#xD;
		$view.list.refresh();&#xD;
	});&#xD;
},&#xD;
buttonEdit_click:function(rowId,rowData,tableData){&#xD;
	if(rowId){&#xD;
		Modal.open(&quot;f/dzjc_tongji/updateJcFilterOrg&quot;,{id:rowId},function(data){&#xD;
			$view.list.refresh();&#xD;
		});&#xD;
	}else{&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}&#xD;
},&#xD;
buttonDelete_click:function(rowId,rowData,tableData){&#xD;
	var ids = $view.list.getSelectRowIds() ;&#xD;
	if(ids.length==0){&#xD;
		Messenger.error(&quot;请选中表格项!&quot;);&#xD;
	}else{&#xD;
		Modal.openDelConfirm({message:&quot;确定是否删除！&quot;},&#xD;
			function(data){&#xD;
				if(ids.length==1){&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_tongji/v1/JcFilterOrg/&quot;+ids[0]).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				}else{&#xD;
					 var queryStr=&quot;&quot;;&#xD;
					 for (var i =0; i&lt; ids.length;i++){&#xD;
						if(i==0){&#xD;
						   queryStr+=&quot;id=&quot;+ids[i] ;&#xD;
						}else{&#xD;
						   queryStr+=&quot;&amp;id=&quot;+ids[i];&#xD;
						}&#xD;
					  }&#xD;
					$http.delete(c2.base(&quot;service&quot;)+&quot;dzjc_tongji/v1/JcFilterOrg?&quot;+queryStr).success(function(){&#xD;
						$view.list.refresh();&#xD;
					});&#xD;
				 }&#xD;
		});&#xD;
	}&#xD;
},
query_button:function($event){&#xD;
	var queryParams = new Array();
	if($model.list.orgId!= '' &amp;&amp; $model.list.orgId!= null){
		queryParams.push({field:&quot;orgId&quot;,op:&quot;cn&quot;,data:$model.list.orgId}) ;
	}
	if($model.list.orgName!= '' &amp;&amp; $model.list.orgName!= null){
		queryParams.push({field:&quot;orgName&quot;,op:&quot;cn&quot;,data:$model.list.orgName}) ;
	}
	if(queryParams.length != 0 ){
		var filters={
			groupOp:&quot;AND&quot;,
			rules:queryParams
		}&#xD;
		$view.list.refresh({filters:filters});
	}else{&#xD;
		$view.list.refresh({});&#xD;
	}
},
clean_button:function($event){
	$view.mainForm.clean();&#xD;
	$view.list.refresh({});
}</form:scripts>
