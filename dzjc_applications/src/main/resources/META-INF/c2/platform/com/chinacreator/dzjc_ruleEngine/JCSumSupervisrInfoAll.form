			&lt;td class=&quot;header sort&quot; rowspan=&quot;3&quot; ng-click=&quot;functions.sort('yellow')&quot; ng-mouseover=&quot;functions.sortStyle()&quot;>黄牌&lt;/td>
			&lt;td class=&quot;header sort&quot; rowspan=&quot;3&quot; ng-click=&quot;functions.sort('red')&quot; ng-mouseover=&quot;functions.sortStyle()&quot;>红牌&lt;/td>
			&lt;td class=&quot;header&quot; rowspan=&quot;2&quot; colspan=&quot;2&quot;>撤销&lt;/td>
	&lt;/tr>
	&lt;tr>
	&lt;/tr>
	&lt;tr>
			&lt;td class=&quot;header sort&quot; ng-click=&quot;functions.sort('cancel_yellow_num')&quot; ng-mouseover=&quot;functions.sortStyle()&quot;>黄牌&lt;/td>
			&lt;td class=&quot;header sort&quot; ng-click=&quot;functions.sort('cancel_red_num')&quot; ng-mouseover=&quot;functions.sortStyle()&quot;>红牌&lt;/td>
	&lt;/tr>

&lt;tbody ng-repeat=&quot;jCSumData in $params.jCSumData&quot;>

		&lt;tr  ng-if=&quot;jCSumData.area_name!='合计'&quot;>
			&lt;th ng-if=&quot;$params.showArea&quot;>&lt;a ng-click=&quot;functions.changeAreaCode(jCSumData)&quot;>{{jCSumData.area_name}}&lt;/a>&lt;/th>
			&lt;th ng-if=&quot;$params.showUnitName&quot;>{{jCSumData.area_name}}&lt;/th>
			&lt;th>{{jCSumData.yjs}}&lt;/th>
			&lt;th>{{jCSumData.yellow}}&lt;/th>
			&lt;th>{{jCSumData.red}}&lt;/th>
			&lt;th>{{jCSumData.cancel_yellow_num}}&lt;/th>
			&lt;th>{{jCSumData.cancel_red_num}}&lt;/th>
		&lt;/tr>

		&lt;tr  ng-if=&quot;jCSumData.area_name=='合计'&quot; style=&quot;color:red; height:35px;&quot;>
			&lt;th >{{jCSumData.area_name}}&lt;/th>
			&lt;th>{{jCSumData.yjs}}&lt;/th>
			&lt;th>{{jCSumData.yellow}}&lt;/th>
			&lt;th>{{jCSumData.red}}&lt;/th>
			&lt;th>{{jCSumData.cancel_yellow_num}}&lt;/th>
			&lt;th>{{jCSumData.cancel_red_num}}&lt;/th>
		&lt;/tr>

		

&lt;/tbody>

&lt;/table>




&lt;/script></form:htmls>
